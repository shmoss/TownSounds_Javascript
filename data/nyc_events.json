var nyc_events = [{"Address": "555 W 42nd St, New York, NY 10036", "Artist": "Comedian Jacob Williams", "ArtistImage": "https://photos.bandsintown.com/thumb/6642242.jpeg", "Coordinates": [-73.997347, 40.760625], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "1:30 PM", "Venue": "Upright Citizens Brigade Theatre", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "235 W 46th St, New York, NY 10036", "Artist": "Harlem Gospel Choir", "ArtistImage": "https://photos.bandsintown.com/thumb/9296682.jpeg", "Coordinates": [-73.987088, 40.759536], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Christian-gospel, Gospel, Christian/gospel", "Time": "1:30 PM", "Venue": "Sony Hall", "moreBioInfo": "No artist bio available", "otherInfo": "The Choir brings their experience of Harlem and the Black Church to Times Square in a foot stomping, hand clapping show for fans from around the world. The home-style \"all you can eat\" southern buffet is guaranteed to fill the hole in your soul, and the Choir's joyous performance will have you dancing on the stage."}, {"Address": "17 Moultrie St, Brooklyn, NY 11222", "Artist": "Sheila", "ArtistImage": "https://photos.bandsintown.com/thumb/8929300.jpeg", "Coordinates": [-73.946256, 40.727806], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Alternative", "Time": "2:00 PM", "Venue": "Leesta Vall Sound Recordings", "moreBioInfo": "Sheila is not just a band but a representation of change that we, three young black women, want to see in ourselves as well as the world. What started as weekly skype sessions between sisters Courtney and Rhea Tucker and Sha'Air Hawkins has quickly transcended into the now formed Sheila. Sheila mixes sounds of alternative rock, R&B, and rap as influences to their sound.", "otherInfo": "No other event info available"}, {"Address": "1 South Ave, Garden City, NY 11530", "Artist": "Grace McLean and Them Apples", "ArtistImage": "https://photos.bandsintown.com/thumb/6388885.jpeg", "Coordinates": [-73.652451, 40.722173], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Pop, Singer, Songwriter, Electronic", "Time": "2:00 PM", "Venue": "Adelphi University Performing Arts Center", "moreBioInfo": "Called \u201celectrifying\u201d by The Huffington Post and \u201cphenomenal\u201d by The New York Times, Grace McLean is a multi-hyphenate actor-singer-writer-teacher on the rise. In addition to performing in the New York theater scene on Broadway and off, Grace also makes time for her acclaimed original music, writing for her band Grace McLean & Them Apples. Manhattan User's Guide writes, \"this pop and jazz singer writes seriously infectious melodies with seriously antic lyrics. Her voice can raise the roof, or she can slide off a note and break your heart,\u201d and Stephen Holden of the New York Times says \u201cMs. McLean\u2019s voice [is] a flexible instrument with unexpected reserves of power...Behind her playful adventurousness lies a well of passion.\u201d", "otherInfo": "No other event info available"}, {"Address": "44 E 32nd St, New York, NY 10010", "Artist": "Carmine", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.982865, 40.74594], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "2:00 PM", "Venue": "The Cutting Room", "moreBioInfo": "No artist bio available", "otherInfo": "$25 food & beverage minimum per person."}, {"Address": "881 7th Ave, New York, NY 10019", "Artist": "Sir Bryn Terfel", "ArtistImage": "https://photos.bandsintown.com/thumb/9161478.jpeg", "Coordinates": [-73.980276, 40.764881], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "2:00 PM", "Venue": "Stern Auditorium / Perelman Stage at Carnegie Hall", "moreBioInfo": "No artist bio available", "otherInfo": "Whether he is singing a dramatic aria or exploring the nuances of a song, Sir Bryn Terfel \u201ccan roar\u2014with pain, with pleasure, with fierce indignation, filling the hall with full, strong tone, [and] can also coo and purr at an extreme pianissimo, making a sound that stays audible only because there is so much in it to feel, as well as to hear\u201d (The New York Times). The beloved Welsh bass-baritone returns to Carnegie Hall for one of the season\u2019s most electrifying recitals. Part of: Great Singers I Performers Sir Bryn Terfel, Bass-Baritone Natalia Katyukova, Piano"}, {"Address": "114 Little York Rd, Warwick, NY 10990", "Artist": "Lauren Davidson Music", "ArtistImage": "https://photos.bandsintown.com/thumb/9448220.jpeg", "Coordinates": [-74.441829, 41.289775], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Urban Country", "Time": "2:00 PM", "Venue": "Warwick Valley Winery & Distillery", "moreBioInfo": "CMA Artist and Grammy Recording Academy Member Lauren Davidson is an Urban Country singer/songwriter who has opened for Old Dominion, LOCASH, Carly Pearce, Justin Moore and others.", "otherInfo": "LD Acoustic Trio"}, {"Address": "444 Saw Mill River Rd, Elmsford, NY 10523", "Artist": "Tom Wardle", "ArtistImage": "https://photos.bandsintown.com/thumb/8995256.jpeg", "Coordinates": [-73.814617, 41.070309], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Summer Pop", "Time": "2:30 PM", "Venue": "Captain Lawrence Brewing Company", "moreBioInfo": "English singer/songwriter giving it a go in the US. The brand new Jacqueline EP coming February 2018.\n\nListen now at www.soundcloud.com/tomwardle", "otherInfo": "Tom makes a return to the Captain Lawrence Brewing Company playing some live acoustic pop and soul in the Brewery all afternoon."}, {"Address": "2 Union Street, Sparkill, NY 10976", "Artist": "OxANA", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.927829, 41.029772], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "3:00 PM", "Venue": "Union Arts Center", "moreBioInfo": "No artist bio available", "otherInfo": "Oxana Mikhailoff and Vassily Primakov present a program that is the most exiting and fun both for the performers and the audience."}, {"Address": "139 W 44th St, New York, NY 10036", "Artist": "Mauro Refosco", "ArtistImage": "https://photos.bandsintown.com/thumb/7531075.jpeg", "Coordinates": [-73.984599, 40.756856], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Rock, Latin", "Time": "3:00 PM", "Venue": "Hudson Theatre", "moreBioInfo": "Mauro Refosco is a professional percussionist and noisemaker who performs and records with artists such as David Byrne, The Red Hot Chili Peppers and Thom Yorke\u2019s Atoms for Peace.", "otherInfo": "No other event info available"}, {"Address": "30 Lincoln Center Plaza, New York, NY 10023", "Artist": "Iestyn Davies - Countertenor", "ArtistImage": "https://photos.bandsintown.com/thumb/8588961.jpeg", "Coordinates": [-73.981696, 40.771089], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Classical Music", "Time": "3:00 PM", "Venue": "Metropolitan Opera House", "moreBioInfo": "\"Iestyn Davies has one of the most glorious countertenor voices in the world today\" Michael Church, The Independent", "otherInfo": "No other event info available"}, {"Address": "715 North Ave, New Rochelle, NY 10801", "Artist": "Kate Warren, Horn", "ArtistImage": "https://photos.bandsintown.com/thumb/10036940.jpeg", "Coordinates": [-73.788241, 40.925243], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Classical, Classical Crossover, Instrumental", "Time": "3:00 PM", "Venue": "Christopher J. Murphy Auditorium", "moreBioInfo": "Kate Warren is a New York City based french horn player. She holds masters and bachelors degrees in music performance from the New School and Florida State University respectively, and has previously held positions with the Tallahassee Symphony Orchestra, Northwest Florida Regional Orchestra, and Sinfonia Gulf Coast.", "otherInfo": "Annual young artists concerto competition winners recital. Program includes:\n-Mozart Piano Concerto in Eb Maj, K.271\n-Mozart Violin Concerto no. 5 in A Maj, K.219\n- Haydn Oboe Concerto in C Maj"}, {"Address": "470 River Styx Rd, Hopatcong, NJ 07843", "Artist": "Richie G and Ginger Martini Duo", "ArtistImage": "https://photos.bandsintown.com/thumb/5343707.jpeg", "Coordinates": [-74.651628, 40.942979], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Rock, Pop Country, Pop, singer songwriter", "Time": "3:00 PM", "Venue": "Patrick's Pub", "moreBioInfo": "So\u2026.first a little history. Sadly, I attempted violin as a young child and was brutally bad. My parents had some how had me listening to classical music at that time which probably explains the violin attempt. Then a miracle happened\u2026First, the Beatles arrived so, bye bye classical & then my aunt who was buying a new piano (the wealthier end of the family) offered me her old one. I\u2019m 8 1/2 so I was still agreeable to anything so I promised to take lessons and really work at it.\n\nI lied\u2026After a year and a half I quit\u2026..until I discovered Elton John. That was it! I was determined to be a musician. In the late 70\u2019s I joined a fairly succesful 50\u2019s/60\u2019s band called Slick. I played 6 nights a week and worked full time at a department store as a stock man (person in this politically correct universe). Exhaustion wasn\u2019t considered. I went to gigs with a thermos of tea and lemon since I was always sick. This lasted until the early 80\u2019s. I joined a band called Loose Change which (under my direction) played anything from the 1960\u2019s- whatever was current. That lasted 12 years. Many years of putting my ears in harms way and breathing in way to much bar smoke told me it was time to quit. This was somewhere around 1993/94 (I don\u2019t remember). The next time I played live was in 2004 at my local block party. That\u2019s what I needed to get the itch to play again but, I was working in retail at Sam Ash Music and simply had no time to gig. In 2007 I was offered a better job which offered me a 9-5 existence. The economy turned sour and earning extra cash became a priority. And then it happened\u2026.I walked into a local Italian restaurant called Vera\u2019s Trattoria to pick up a pizza and lo and behold (fancy words ehh?) a piano player was pumping out Jazz licks. Can you imagine? Live music! I decided to approach the owner to see if I could score a gig. He decided to try me the following week. I was hired as a permanent fixture on Saturday nights. Thus The Richie G Review was born. \u201cA one man band Piano/vocals using backing tracks and a voice harmonizer to create a full band sound. My current song list represents over seventy artists including Elton John, Billy Joel , James Taylor, The Beatles and hits from the 1960\u2019s thru today. Over 500 tunes and counting\u2013 Booking email onemanbandrichieg@gmail.com .\u201d I played at Vera\u2019s for 14 months and then a new owner took over. He decided that entertainment was unnecessary. I got to work recording a CD of covers and creating business cards. The hard work paid off! I\u2019m a permanent Wednesday night fixture at the Woodcliff lake Hilton as well as sprinkling in various weekend gigs.\n\nRidgewood Street Fair 2013-2014\n\n\n\n392561_413258562028352_398683286819213_1318553_160572558_nRIDGEWOOD\n\n\n\nChapter 2- Ginger Martini was Born Right around St Patty\u2019s Day of 2012, I was performing at the Elm Street Grill in Oakland NJ. I was approached by my soon to be partner Wendy Kelly. I was on the floor wiring up my equipment as she, asked me questions about what I do (kind of music, etc). I was excited believing that I was going to be hired for a private party. During the break, she handed me her card.It turned out she was interested in joining me. I wasn\u2019t looking but, I agreed to see what she sounded like at my rehearsal space (haha my basement). Three songs later I was convinced. We sounded great together. A week later, we had 50 plus songs ready for prime time. She joined me at The Paris Inn in Wayne NJ. Ever since we perform around 50% of my gigs together and get an incredible response. Wendy started a seasoned veteran of the Broadway, Standard, and Country Genres. She has since dragged me into her country word as I dragged her into my pop/rock world. An amazing combination was born!", "otherInfo": "No other event info available"}, {"Address": "1027 Flatbush Ave, Brooklyn, NY 11226", "Artist": "Romeo & Juliet", "ArtistImage": "https://photos.bandsintown.com/thumb/9619534.jpeg", "Coordinates": [-73.95724, 40.64599], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: International, Russian, Pop", "Time": "3:00 PM", "Venue": "Kings Theatre", "moreBioInfo": "No artist bio available", "otherInfo": "ROMEO AND JULIET Full-Length Ballet in 2 Acts Music by Sergei Prokofiev Choreography by Michael Lavrovsky Production by Garry Savoyan Juliet - Olena Dobrianska Prima - Ballerina Romeo - Sergii Dotsenko Principal Dancer This full-scale production set to the music of Sergey Prokofiev and based on William Shakespeares timeless tale of tragic love. The National Ballet Theater of Odessa Ukraine bring 55 of Ukrainian talented and brightest Ballet stars to present the most passionate romantic tragedy of love."}, {"Address": "New York City, NY", "Artist": "Mickey Utley Band", "ArtistImage": "https://photos.bandsintown.com/thumb/8488718.jpeg", "Coordinates": [-73.9808, 40.7648], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "3:00 PM", "Venue": "Norwegian Cruise Lines - Bliss", "moreBioInfo": "Mickey Utley Band", "otherInfo": "No other event info available"}, {"Address": "35 Hyatt Street, Staten Island, NY 10301", "Artist": "Casablanca", "ArtistImage": "https://photos.bandsintown.com/thumb/6046251.jpeg", "Coordinates": [-74.077461, 40.641921], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Indie Pop, Indie, Hard Rock, Pop, Rock, Alternative", "Time": "3:00 PM", "Venue": "St. George Theatre", "moreBioInfo": "No artist bio available", "otherInfo": "Join us for a Oscar Night pre party and screening of the classic Academy Award Winning 1942 film Casablanca as part of the 2020 Better on the Big Screen Classic Movie Series sponsored by Williams Eye Works and Richmond University Medical Center. Fill out your Oscar Ballot for a chance to win season passes for two to the 2020 Better on the Big Screen Classic Movie Series"}, {"Address": "511 Warburton Ave, Yonkers, NY 10701", "Artist": "Gamelan Dharma Swara", "ArtistImage": "https://photos.bandsintown.com/thumb/9186753.jpeg", "Coordinates": [-73.8961, 40.95001], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Traditional, Contemporary And Experimantal Works For Balinese Music And Dance", "Time": "3:00 PM", "Venue": "Hudson River Museum", "moreBioInfo": "Om Swastiastu", "otherInfo": "Kick off the Year of the Rat with traditional Asian arts, crafts, and demonstrations for all ages! Gender wayang and Balinese dance performances begin at 3pm.\nFree with general admission to museum."}, {"Address": "6 S Monroe St, Ridgewood, NJ 07450", "Artist": "Bassoon", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-74.127455, 40.982094], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz", "Time": "3:00 PM", "Venue": "West Side Presbyterian Church", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "1200 NY-25A, Stony Brook, NY 11790", "Artist": "James Maddock", "ArtistImage": "https://photos.bandsintown.com/thumb/8623110.jpeg", "Coordinates": [-73.141702, 40.912316], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Americana, Folk, Rock, Epic", "Time": "3:00 PM", "Venue": "The Long Island Museum", "moreBioInfo": "James Maddock affirms his status as a fixture in downtown Manhattan\u2019s folk and Americana scene with \u2018Another Life,\u2019 his new album out July 23rd on Jullian Records. As NPR says of the British transplant, \u201chis timeless songwriting style, which seems to draw from great songwriters of every era, conveys quiet confidence and lovely intimacy.\u201d\n\nNodding to \u201870s rock with shimmering acoustic guitars, an understated rhythm section and flourishes of fiddle, mandolin, piano and dobro, \u2018Another Life\u2019 bridges the mainstream appeal of songwriters like Jesse Harris and Norah Jones and the quirkier sensibility of Loudon Wainwright. With funding entirely from fan contributions through PledgeMusic, Maddock worked with producer Matt Pierson, multi-instrumentalist Larry Campbell (who\u2019s played with Bob Dylan and Levon Helm) and bassist Tony Scherr (who plays with Norah Jones).\n\nThe 13 songs on 'Another Life' paint a portrait of a man reaching the midpoint of his life, a natural progression from themes in Maddock's earlier releases. He\u2019s looking back on roads not taken, and isn\u2019t always sure he chose the right one. On the title track, Maddock wants an extra 50 years to try \u201cacting, white-water rafting,\u201d \u201csome illegal substances,\u201d even having a kid or three. \u201cI\u2019ve Been There Too,\u201d built around an indelible guitar riff, shows Maddock\u2019s knack for instrumental hooks as well as lyrical ones. \u201cDon\u2019t Go Lonely,\u201d in a well-timed moment of levity, hints at reggae with dub bass, bubbling organ and the chorus\u2019s entreaty to \u201cgo get high, go get found, go crazy on this town / don\u2019t go lonely.\u201d\n\nAfter fronting the Columbia Records band Wood, whose debut \u2018Songs From Stamford Hill\u2019 was featured prominently on TV shows such as \u201cDawson\u2019s Creek,\u201d Maddock moved from England to New York City in 2003. His 2009 album \u2018Sunrise On Avenue C\u2019 won a New York Music Award for Best Americana Album, while follow-up 'Wake Up And Dream' ranked among the top albums of 2011 in WFUV's (NYC) Listener Poll. He's performed with Bruce Springsteen, Willie Nile, Aaron Comess (Spin Doctors) and David Immergluck (Counting Crows), sang on an album with Susan McKeown, and is a resident artist at Rockwood Music Hall. According to radio legend Vin Scelsa, Maddock's \"heartbreakingly beautiful and exquisitely crafted\" music \"touches the soul.\"\n\nA seasoned live performer, Maddock will play songs from 'Another Life' at shows in New York City and across the Northeast this summer, as well as at festivals in the U.S. and Europe.", "otherInfo": "No other event info available"}, {"Address": "15 Vandam St, New York, NY 10013", "Artist": "Chronicles", "ArtistImage": "https://photos.bandsintown.com/thumb/6302171.jpeg", "Coordinates": [-74.00995, 40.72446], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Metal, Progressive Metal", "Time": "7:30 PM", "Venue": "Huron Club At Soho Playhouse", "moreBioInfo": "No artist bio available", "otherInfo": "Feuding tailors, a cocaine-addicted rabbi and a Jewish woman who joins a fundamentalist Christian sect are just a few of the sometimes poignant, often humorous stories told through music in singer-songwriter Daniel Cainer's Jewish Chronicles. Winner of an Edinburgh Fringe Award, Cainer's one-man musical journey has been a smashing success from London to New York to Sydney thanks to his deft storytelling ability and virtuoso piano chops. You'll laugh, cry and thrill to this one-man masterpiece, which the U.K.'s Daily Mirror called a must see."}, {"Address": "145 Westchester Ave, Port Chester, NY 10573", "Artist": "Natasha DiMarco", "ArtistImage": "https://photos.bandsintown.com/thumb/9027859.jpeg", "Coordinates": [-73.665244, 41.0014], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz, Reggae, Soul, Funk, Folk, Classic Rock", "Time": "3:00 PM", "Venue": "Garcia's", "moreBioInfo": "Natasha DiMarco is a vibrant and soulful voice on the scene. She contains an old soul yet delivers with boundless passion and vigor. Her style effortlessly weaves jazz, rock, reggae, funk, folk, soul & blues together. She sings with the Bobby Deitch Band, I Anbassa, Denroy Morgan, Anita Antoinette (of the Voice), Dark Side Of The Hudson, Nigerian artist Lamboginny and recently released a straight ahead jazz record, \"Prelude\", with the Al DiMarco Swing Band.", "otherInfo": "An inclusive concert considering those with special needs & their families. $15"}, {"Address": "720 Northern Blvd, Brookville, NY 11548", "Artist": "Drum Tao", "ArtistImage": "https://photos.bandsintown.com/thumb/9617488.jpeg", "Coordinates": [-73.595225, 40.81951], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Other", "Time": "4:00 PM", "Venue": "Tilles Center - Krasnoff Theater", "moreBioInfo": "No artist bio available", "otherInfo": "Tilles Center at LIU Post & Capital One Presents: Sunday, February 9th 2020 Drum Tao AT Tilles Center Concert Hall LIU Post 720 Northern Boulevard Brookville, NY 11548 4:00PM Tickets can be purchased at the box office Monday - Saturday from 1PM - 6PM or by calling 516-299-3100 PLEASE NOTE: Any tickets purchased over the phone or online are subjected to additional fees"}, {"Address": "125 St Ann's Ave, Bronx County, NY 10454", "Artist": "The O", "ArtistImage": "https://photos.bandsintown.com/thumb/10076363.jpeg", "Coordinates": [-73.919849, 40.803992], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "4:00 PM", "Venue": "The Living Room Il", "moreBioInfo": "South Carolina Native with a work ethic that is out of this world. The O has been uplifting the hip hop lovers with the sounds that rock the house!! The o has 15 years of hip hop experience performing with the who's who of the industry. So get ready because The O is making his impact on the industry as you read this !!! https://itunes.apple.com/us/album/birthday-girl-feat.-nature/id591942017 Download The O single BirthDay Girl Charleston Original Hip Hop artist!! https://open.spotify.com/album/6vZ6SWK1nXUZYQU1FLlvqj listen Now", "otherInfo": "Come out for the taping to witness some hot 165, Cipher Rounds, Features, & Meet Our Sponsors! The O live!"}, {"Address": "201 S Division St, Peekskill, NY 10566", "Artist": "Diana Flynn", "ArtistImage": "https://photos.bandsintown.com/thumb/8888597.jpeg", "Coordinates": [-73.919683, 41.289461], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Dark Pop, Singer, Songwriter", "Time": "4:00 PM", "Venue": "Bean Runner Cafe", "moreBioInfo": "Singer of Sad Songs", "otherInfo": "No other event info available"}, {"Address": "90 Wythe Ave, Brooklyn, NY 11249", "Artist": "Mister White", "ArtistImage": "https://photos.bandsintown.com/thumb/10078129.jpeg", "Coordinates": [-73.958254, 40.721704], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Hip Hop, Funk, Soul, Instrumental, Latin, Disco", "Time": "4:00 PM", "Venue": "Kinfolk 90", "moreBioInfo": "No artist bio available", "otherInfo": "In The Clouds collective playing live at Kinfolk 90. Music by: Quinoa Jones, Wally Wonder Mister White & Sleepyjeanson"}, {"Address": "125 St Ann's Ave, Bronx County, NY 10454", "Artist": "Serious Voice of New York", "ArtistImage": "https://photos.bandsintown.com/thumb/8631953.jpeg", "Coordinates": [-73.919849, 40.803992], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Hip Hop, Christian Rap, Rap", "Time": "4:00 PM", "Venue": "The Living Room Il", "moreBioInfo": "Serious Voice is a female rapper/singer who brings a heavy dose of hip hop, laced with new school and old school tones. Classified as an inspirational rapper, Serious is often told she sounds and looks like Grammy rapper/producer Missy Elliott, but Serious is just a different brand of artist once you partake in the \"experience\". The award-winning rapper was recently showcased in national publication ESSENCE MAGAZINE for her versatility. In March 2016, her music video for Thirsty, which has garnered over 18,000 views was showcased at the SXSW Video Screening for Wix.com.\n\nSerious knew from the age of 12 that she was created to rap. She didn\u2019t know how things would work out but kept working on her craft.\n\nShe was 21 years old, on missionary work in Johannesburg, South Africa and was asked by the perform at a stadium concert. After giving up on her gift for over 10 years she was very reluctant, but was obedient. That day Serious impacted many and knew that she would never give up on her gift.\nAfter her missionary work was complete, she headed home to record YES, a song tha took many locals in the New York Tri-state area by storm in 2000.Then in 2009 she felt the very strong urge to take her music to the next level and began to seek out better producers, management and the like.\nIn 2011 she signed a deal with Holywood Records, a Florida based label. During that year she auditioned for the Staten Island Gospel Fest and became a finalist and was honored to minister to folks from across the tri-state area. That same year, coordinators at WIADCA, a notable \u201csecular\u201d Caribbean organization that furnished scholarships to youngsters in addition to taking part in the Labor Day festivities in NYC invited Serious as the first and only female Christian rapper to perform on the Brooklyn Museum stage for the WIADCA GospelFest.\nSerious released Are You Serious Vol 1 in January 2012. The mixtape was available on all major Christian hip hop sites and Datpiff, garnering over 11k downloads.\nIn August of 2012, Serious was nominated as Female Rap Minister by the Kingdom Choice Awards Committee. Since the nomination, she has successfully toured various parts of Texas with her SAAB message, touching the hearts of women by simply testifying and sharing her music.\nNow under the label Serious PPL Music, which is headed by Christopher James, Serious continues to tour and do local and national shows. She is currently working on her debut album and is on her three year long tour called (Stilettos & A Book) SAAB.", "otherInfo": "No other event info available"}, {"Address": "Asbury Park, Asbury Park, NJ 07712", "Artist": "Christopher Paul Stelling", "ArtistImage": "https://photos.bandsintown.com/thumb/9518993.jpeg", "Coordinates": [-74.01, 40.22], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Folk, Folk Revival, Singer, Songwriter, Anti-Folk, Americana", "Time": "4:00 PM", "Venue": "Danny Clinch Transparent Gallery", "moreBioInfo": "Christopher Paul Stelling\nBest of Luck\n\n\u201cIt\u2019s not Americana or alt anything, it\u2019s really just soulful folk music,\u201d Ben Harper says of Best of Luck, the new album by Christopher Paul Stelling, which he produced.\n\nHarper and Stelling met a few years back when the Grammy award winning musician-producer invited Stelling to open a series of shows. \u201cBen gave me a true gift back then,\u201d Stelling says. \u201cI\u2019d been on the road for a long time and he put me onstage in front of his fans. He took me to the Beacon, The Ryman, Massey Hall, all these leg-endary rooms. Just to see that what I could do would even translate in spaces like that was revelatory.\u201d\n\nHarper says he instantly recognized a kindred spirit in Stelling\u2019s virtuosic finger picking and soulful delivery. \u201cIt was like finding a John Fahey or Leo Kottke that was a really great singer,\u201d he explains.\n\nThe recording of Best Of Luck culminated on a rainy night in Los Angeles, but the whole thing really began back in North Carolina. Stelling calls Asheville home, though he has been in a state of near constant motion for years. Leaving Florida while young to roam and perfect his craft, he made stops in Colorado, Boston, Seattle, New York and eventually North Carolina - all interspersed with relentless touring - hundreds of performances a year, in bars, coffee houses, nightclubs and theaters.\n\n\u201cThe lifestyle of constant touring was gaining on me,\u201d Stelling says. \u201cAnd then I was back home, and in the abrupt silence, there was a clarity. The fumes of hubris I had been running on for years were all but gone and my relationship with my environment and myself had turned toxic. I was scared but resolved to persevere. On Christmas Day, after a few false starts, I gave up drinking and decided to take a look at myself.\u201d\n\nAnd then, as if following some greater cosmic narrative, an email arrived that New Year\u2019s Day. It was Harper asking if he would be interested in making a record in Los Angeles. \u201cI had never worked with a producer before,\u201d Stelling says. \u201cI had always con-trolled everything; it was how I had survived. But I needed a mentor and a friend; I was too far from shore.\"\n\nStelling accepted an artist\u2019s residency at the Stetson Kennedy House outside of Jack-sonville Florida to prepare. Kennedy had been a folklorist and comrade of Woody Guthrie and Zora Neale Hurston. Woody stayed there for extended periods as well. \u201cI moved into this little house in a North Florida swamp just an hour from where I was born and raised,\u201d Stelling says. \u201cAnd I proceeded to freak out. It was the first time I\u2019d just sat still for years. Mostly, I took inventory of myself. But I left feeling like I\u2019d found a thread. After that, I would send Ben demos. He would respond with encouragement and tell me to keep digging, so I\u2019d keep digging. Looking back, the whole process was the medicine I needed.\u201d\n\nBest Of Luck is a supremely accessible and finely crafted record that deftly merges genres. Harper, who has previously produced records by Mavis Staples, Rickie Lee Jones, The Blind Boys of Alabama and others, recruited an all-star rhythm section with Jimmy Paxson (Stevie Nicks, Dixie Chicks) on drums and upright bass player Mike Va-lerio (Randy Newman, LA Philharmonic) to lend a versatility and finesse. \u201cI really be-lieve this record is the intersection where folk and soul meet,\u201d Harper says.\n\nThe album\u2019s title, Best Of Luck, mirrored perfectly the emotional landscape in which it was created. \u201cDepending on how you say it, it can either be a blessing or a dismissal,\u201d Stelling says. \u201cAnd that was exactly the point I was coming to with myself, my career as an artist, as a friend and as a person.\u201d\n\nThe first track, \u201cHave To Do For Now\u201d is a haunting evocation of a moment between an unexamined past and an uncertain future. The guitar refrain evokes a beautiful sense of wistfulness about the past. \u201cIt\u2019s a song about the fear of losing memories and the re-lief of letting them go,\u201d Stelling explains. \u201cIt was written in one sitting. A flood of memo-ries moving back to when I was very young and I fell from a tree house and split my head open. I\u2019d never really processed that trauma but it\u2019s one of my earliest and most visceral memories. I can\u2019t help but wonder how that day and others like it have influ-enced my life.\u201d\n\nThe sonic simplicity of \u201cLucky Stars\u201d reveals a richness of melody and emotionality. \u201cIt\u2019s part love song, part lullaby,\u201d Stelling says of the track, \u201cwith a gentle touch from the band and Ben\u2019s haunting lap steel matching my guitar in the instrumental section, ad-ditional vocals by my partner. It\u2019s about gratitude and the mysteries of the universe. Even to just be here and marvel at it all is enough for me most times. Even if it\u2019s mean-ingless, it\u2019s beautiful.\u201d\n\nThroughout the record, discontent and self-doubt are transformed into messages of resilience and hope. \u201cTrouble Don\u2019t Follow Me\u201d captures this perfectly, with an upbeat almost anthemic rhythm and soulful, gospel-tinged vocals. \u201cIt comes as no surprise to me that one of my most optimistic songs could be written in the most difficult hour,\u201d Stelling says. \u201cWhen all I needed was a little hope and a song I could play night after night and not get tired of. Something that captured the basic recipe for endurance. A song about marching on, a warning to anything that might stand in your way, and needing to feel strong for the people around you, so you can inspire them to do the same.\u201d\n\nThere is an uninhibited defiance to \u201cUntil I Die,\u201d a revved up folk-blues featuring a beautifully raw guitar and beat. \u201cThis song is a kaleidoscope in the hall of mirrors, a psychedelic rant,\u201d Stelling says. \u201cOne thing is for certain amidst the chaos and uncer-tainty, and that is having someone to love and who loves you back is unrivaled.\u201d\n\nWhen Harper talks about his admiration for Stelling, he refers to his family\u2019s legendary folk music store, filled with instruments and a fluid merging of styles and genres. \u201cWhen I listen to Chris, I hear my grandparent\u2019s store in Claremont,\u201d Harper says. \u201cHe\u2019s carrying on that tradition. A guy that can play any instrument from any country and play it with real feeling. He crosses genres but manages to respect them; he is a folk singer with an unusual soulfulness. He understands where this music comes from and why it\u2019s remains so essential. Of all the record\u2019s I have produced, this is one I\u2019m most proud of.\u201d\n\nFor his part, Stelling says, \u201cThis record was about learning to trust the people around me. If anything, after years of closely guarded decision-making I\u2019m delighted by the re-sults of this collaboration. This record has an undeniable sense of clarity. The songs are lean but never feel incomplete. Ben knew how to coax me out of my well-worn comfort zones, how to move fast and spontaneously and put me on the spot, and how to retreat and give me space to process things. People who know me have said they hear a lot more of me in this album than anything before. Realizing I couldn\u2019t heal the world, I started to heal myself, and I couldn\u2019t be more pleased by how that sounds.\u201d", "otherInfo": "No other event info available"}, {"Address": "1121 Elizabeth Ave, Elizabeth, NJ 07201", "Artist": "Manny Cabo", "ArtistImage": "https://photos.bandsintown.com/thumb/9101761.jpeg", "Coordinates": [-74.212344, 40.662132], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Southern Pop Rock, Rock, Pop Rock, Classic Rock, Commercial Rock, Latin", "Time": "4:00 PM", "Venue": "C. F.C.", "moreBioInfo": "\"Ranging to the edgier end of the pop rock continuum, Manny Cabo's music exhibits multiple genre influences but offers a consistently emotive experience through his expertly crafted tracks and Iconic Voice!\"", "otherInfo": "No other event info available"}, {"Address": "25 Drexel Dr, Bay Shore, NY 11706", "Artist": "Nate Charlie Music", "ArtistImage": "https://photos.bandsintown.com/thumb/8983203.jpeg", "Coordinates": [-73.263477, 40.761098], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Acoustic Alternative, Acoustic, Singer, Songwriter, Pop, Pop Punk, Classic Rock, Soft Rock, Rock, Alternative, Indie Pop, Reggae, Folk, Indie Rock, Indie Folk", "Time": "4:00 PM", "Venue": "Great South Bay Brewery", "moreBioInfo": "Nate Charlie is an acoustic singer/songwriter act that performs a variety of cover songs (from the 60's to now) in his own unique way. Occasionally, Nate Charlie teams up with Frank Justin Stark to perform as the Charlie Justin Duo which adds both vocal harmonies and world percussion.\n\nViewers have complimented Nate Charlie\u2019s clever song arrangements, his aim to stand out from the rest, and his focus to keep a fresh face on that of the acoustic entertainment world. Spanning over 50 venues on Long Island and averaging 150 shows played per year, it\u2019s safe to say Nate Charlie has become a staple act in the Long Island music scene.", "otherInfo": "No other event info available"}, {"Address": "1160 Broadway, New York, NY 10001", "Artist": "Jamile Staevie Ayres", "ArtistImage": "https://photos.bandsintown.com/thumb/9238883.jpeg", "Coordinates": [-73.988693, 40.744678], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz, Brazilian Music", "Time": "4:00 PM", "Venue": "Jazz Gallery", "moreBioInfo": "Emerging Jazz vocalist Jamile is a recent graduate of the inaugural Master's of Jazz Studies class at City College of New York. Originally from Brazil, she has been a working vocalist on several venues and has had the opportunity to be a featured vocalist on the show 'Eu Sou Maria (Nativitaten)', the largest known outdoors Christimas project in the world, for a total of 500,000 people.\n\n\n\nCurrently based in New York, she has had the opportunity to perform at Minton's Playhouse with the Antonio Ciacca's Swing Society; Smalls Jazz Club with the Scott Reeves Orchestra feat. Steve Wilson, and with the Antonio Ciacca Quartet; and at Jazz At Lincoln Center with the CCNY Graduate Ensemble, along with several other venues throughout the city. Jamile has also received high praise for her performances at the La Spezia Jazz Festival in Italy. Jamile captivates audiences with her unique phrasing influenced by her Brazilian roots that also reaches the tradition of Sarah Vaughan and Carmen Mcrae.", "otherInfo": "No other event info available"}, {"Address": "44 Berry St, Brooklyn, NY 11211", "Artist": "The Cafe Society", "ArtistImage": "https://photos.bandsintown.com/thumb/8283629.jpeg", "Coordinates": [-73.956571, 40.721058], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz, Swing, Blues, Jazz Blues, Jazz Instrument", "Time": "4:30 PM", "Venue": "The Counting Room", "moreBioInfo": "In the 1940s, there was one integrated jazz club in New York City where all the greats played: Caf\u00e9 Society in the West Village.\n\nToday, The Caf\u00e9 Society is a group of jazz musicians in Brooklyn, NY, led by bassist Jim Greene, saxophonist Justin Flynn, and vocalist Emily Wade Adams. Together with some of the city's most sought-after musicians, they deliver soulful, classic jazz standards without any pretense.\n\nVenues/Clients include E&J Gallo Wines, Feinstein's/54 Below, The Roxy Hotel, The Green Building, Liquor.com, Winnie's Jazz Bar at the Refinery Hotel, Bowery Hotel, Gin Parlour at the Intercontinental New York Barclay Hotel, The Core Club\n\nVocals / Emily Wade Adams\nSax / Justin Flynn\nBass / Jim Greene", "otherInfo": "No other event info available"}, {"Address": "196 Allen St, New York, NY 10002", "Artist": "LT WADE", "ArtistImage": "https://photos.bandsintown.com/thumb/10062022.jpeg", "Coordinates": [-73.98907, 40.72277], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Alternative, Rock", "Time": "5:00 PM", "Venue": "Rockwood Music Hall", "moreBioInfo": "LT Wade is an English musician and producer who began his professional career as guitarist for \u2018Courtney Love\u2019 in Los Angeles In 2007 before moving back to London to join forces with former members of \u2018Black Wire\u2019 to form \u2018Lord Auch\u2019.\n\nIn 2013 LT formed FURS as songwriter and producer with Olly Betts of \u2018The Duke Spirit\u2019 on drums and Sister/vocalist Elle Wade. FURS released several singles leading up their debut album 'Just Kids' that dropped in the summer of 2016.\n\nOver the course of his career LT has been on the road with indie legends \u2018Johnny Marr\u2019 (The Smiths, Modest Mouse, The Cribs), Gaz Coombes (Supergrass), Allah Las, Ducktails. Played legendary European festivals Glastonbury, Secret Garden Party, Reading and toured in USA, Asia, UK and Europe.\n\nLT moved to San Francisco from London in 2016 to embrace the West Coast music scene and embark on a new solo project, writing and recording most of his debut album in California before relocating to Brooklyn, NYC to finish recording and mix his debut record.\n\nDebut Single \u2018Hard To Know\u2019 was released in UK, Europe in 2017 leading up to releasing the full length record \u2018Transient\u2019 due July 27th 2018 on his own label Dopeness Records NYC.", "otherInfo": "Stripped back set with Lowpines"}, {"Address": "445 Albee Square W, Brooklyn, NY 11201", "Artist": "Tim Armacost", "ArtistImage": "https://photos.bandsintown.com/thumb/9505016.jpeg", "Coordinates": [-73.98736, 40.692], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz", "Time": "5:00 PM", "Venue": "Dekalb Market Hall", "moreBioInfo": "Tim Armacost has established himself as one of the most important improvisers on the New York scene today. Armacost was born in Los Angeles, but came of age as a musician in Tokyo, New Delhi and Amsterdam, where he burnished his reputation as an upcoming talent, with a big tenor sound and an impeccable rhythmic sense.\n\nArmacost\u2019s career is distinguished by performance and recording credits alongside the likes of Al Foster, Jimmy Cobb, Kenny Barron, Tom Harrell, Billy Hart, Victor Lewis, Jeff \u201cTain\u201d Watts, Peter Erskine, Ray Drummond, Roy Hargrove, Paquito D\u2019Rivera, Claudio Roditi, Bruce Barth, Dave Kikoski, Don Friedman, Lonnie Plaxico, Robin Eubanks, Charlie Shoemake, Pete Christlieb, Randy Brecker, Akira Tana, Valery Ponomarev, the Maria Schneider Orchestra, and the David Murray Big Band. He has toured throughout East and West Europe, Japan, India, Australia, China and the United States.", "otherInfo": "No other event info available"}, {"Address": "220 W 44th St, New York, NY 10036", "Artist": "DJ Dawson", "ArtistImage": "https://photos.bandsintown.com/thumb/10042768.jpeg", "Coordinates": [-73.98757, 40.7579], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: House, Deep house, Disco, Tech House, Electronic, Electro House", "Time": "5:00 PM", "Venue": "Blue", "moreBioInfo": "No artist bio available", "otherInfo": "Join the ULTRAMAROON crew every other Sunday for a healthy serving of our signature good vibes at Blue Midtown! But wait, there\u2019s more! This season we\u2019ve asked some of our favorite local and international DJ friends to join us as musical guests, and we\u2019ll be announcing each collaboration as we open up RSVPs about a week ahead of each party.\n\nNext up is Sunday, February 9 with Iman Le Caire and her fabulous crew, Vivacious, and musical guests Dawson and Johnny I!\n\nMusic by\nDJ RAF\nDJ Dawson & Johnny I"}, {"Address": "702 Union St, Brooklyn, NY 11215", "Artist": "Jena Friedman", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.980048, 40.676115], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "5:00 PM", "Venue": "Union Hall", "moreBioInfo": "No artist bio available", "otherInfo": "Jena Friedman: Miscarriage of Justice"}, {"Address": "250 E 79th St, New York, NY 10075", "Artist": "Nefesh Mountain", "ArtistImage": "https://photos.bandsintown.com/thumb/9155106.jpeg", "Coordinates": [-73.955473, 40.773341], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Bluegrass, Old Time", "Time": "5:00 PM", "Venue": "Temple Shaaray Tefila", "moreBioInfo": "Nefesh Mountain is the place where American Bluegrass and Old-time music meet with Jewish Heritage and tradition. Band leaders, genre-pioneers, and husband and wife Doni Zasloff and Eric Lindberg are the heart of this eclectic offering, and share their love for American music, their own cultural heritage, and each other with audiences throughout the world. The result of this unexpected and beautiful mix is staggering; and while complete with the kind of adept string virtuosity and through composed arrangements one would hope for from a newgrass band with influences from Bluegrass, Old-Time, Celtic, and Jazz, they also play and sing songs of the heart creating music with a sense of diversity, oneness, and purpose for our world today.\n\nTheir newest and most adventurous recording to date called \u201cBeneath The Open Sky\u201d was a tour de force for the band, featuring friends and bluegrass luminaries Sam Bush, Jerry Douglas, Tony Trischka And David Grier, along with New York based musicians Alan Grubner and Tim Kiah. The album\u2019s 11 songs collectively weave together a groundbreaking and beautiful patchwork of new American music, being called \u201crefreshingly eclectic\u201d by Rolling Stone, and \u201cOne of the finest, wholly bluegrass records one will hear in not only 2018 but as a touchstone moving forward\u201d by No Depression magazine.", "otherInfo": "No other event info available"}, {"Address": "212 E 52nd St, New York, NY 10022", "Artist": "Yael & Gabriel", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.969227, 40.756686], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "5:00 PM", "Venue": "Club Bonafide", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "570 Jernee Mill Rd, Sayreville, NJ 08872", "Artist": "HOLY FAWN", "ArtistImage": "https://photos.bandsintown.com/thumb/9392399.jpeg", "Coordinates": [-74.354439, 40.440976], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Rock, Alternative, Shoegaze", "Time": "5:30 PM", "Venue": "Starland Ballroom", "moreBioInfo": "Four creatures making loud heavy pretty noises.", "otherInfo": "Thrice \"Vheissu\" 15th Anniversary Tour With MewithoutYou, Drug Church, and Holy Fawn"}, {"Address": "196 Allen St, New York, NY 10002", "Artist": "Eden James", "ArtistImage": "https://photos.bandsintown.com/thumb/9059006.jpeg", "Coordinates": [-73.98907, 40.72277], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Rock, Pop, Indie, New Classic Rock", "Time": "5:30 PM", "Venue": "Rockwood Music Hall", "moreBioInfo": "Eden James' single, Never Setting Sun, stole the Number One spot from pop heavyweight Robbie Williams in the Greek Top 40. His debut album of the same name not only gained him chart success and critical acclaim, but also won him the Best New Talent award at Australia\u2019s Sunnie Awards.\n\nAfter the release of his second album Electric Charge, he received the Best International Artist award at Australia\u2019s MUSICOZ Awards, and also picked up two British award nominations at the Indy Music Awards for Best Rock Act and Best International Act.\n\nSoon after, Eden James appeared on the bill with The Killers, Placebo and Moby at the Rockwave festival in Athens. Eden and his band were the second top billing on the Vibe stage for the largest music festival in Greece.\n\nEden James' debut album Never Setting Sun launched him as an international artist, and his performances in Japan, United Kingdom, France, Norway, Greece, Poland, Switzerland, Australia, New Zealand and the USA have resulted in an ever-growing fan base.", "otherInfo": "Eden James gives a special NYC performance to celebrate the release of his new single, Black Book - the 5th single from his forthcoming album.\n\nFree entry. $5/$10 donations are welcomed.\n\nMerch Giveaways on night.\n\nPlease RSVP here - https://facebook.com/events/s/eden-james-single-release-show/482411009129270/?ti=icl\n\nDoors: 5:30 PM\nShowtime: 6:00 PM"}, {"Address": "217 E Houston St, New York, NY 10002", "Artist": "Onlyness", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.986751, 40.722165], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "5:30 PM", "Venue": "Mercury Lounge", "moreBioInfo": "No artist bio available", "otherInfo": "Onlyness, Field Guides, Rose Blanshei"}, {"Address": "315 West 44th St, New York, NY 10036", "Artist": "Birdland", "ArtistImage": "https://photos.bandsintown.com/thumb/183992.jpeg", "Coordinates": [-73.989689, 40.759], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Alternative, Punk", "Time": "5:30 PM", "Venue": "Birdland", "moreBioInfo": "No artist bio available", "otherInfo": "Named after its first regular headliner, Charlie Yardbird Parker, Birdland first feathered its nest on Broadway in 1949, and has since become synonymous with New York jazz. Count Basie and his band made the club their home, as did Coltrane, Monk, Dizzy, Miles and more. Now on West 44th in Midtown Manhattan, Birdland's still going strong, and is called close to perfection for serious fans and musicians by The New York Times, with its awesome sight lines, pristine acoustics and an award-winning Cajun-American menu. You'll find the most famous names in jazz on its stage, as well as exciting up-and-comers every night of the week."}, {"Address": "570 Jernee Mill Rd, Sayreville Station, NJ 08872", "Artist": "mewithoutYou", "ArtistImage": "https://photos.bandsintown.com/thumb/9480815.jpeg", "Coordinates": [-74.354439, 40.440976], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Indie, Experimental, Post Hardcore", "Time": "5:30 PM", "Venue": "Starland Ballroom", "moreBioInfo": "It is no mistake that mewithoutYou have become one of today\u2019s most fascinating experimental rock acts. The last 15 years have borne witness to the Philadelphia five-piece exercising stylistic evolutions and aerial dynamics with humbling dexterity and untamed ambition. At their roots may be a theatrical progressive punk/post-hardcore band, but they\u2019ve never been content to remain comfortably within a familiar genre. Their continuous multi-directional movements have left them increasingly difficult to classify, the growth of their branches impossible to predict. The group\u2019s sixth full-length album, Pale Horses, is the best evidence to date of their eclectic agility.\nThe one constant in mewithoutYou's storied career has been lead singer Aaron Weiss' ability to sketch ornate, thought-provoking narratives. Seamlessly weaving his signature holler amidst whispered storytelling and stream-of-consciousness outpourings, his latest offerings vacillate between the emotionally wracked, vibrantly symbolic, and ambiguously metaphysical. His meandering, technicolor vision of a world apocalyptic\u2014populated with werewolves and vulturemen, shape-shifters and apparitions, android whales and an Idaho bride\u2014combines the fantastic opulence of the group\u2019s recent albums with the vulnerable personal confessions of their earliest work.\nLongtime band-mates Mike Weiss, Rickie Mazzotta and Greg Jehanian continue to craft dramatic, nightmare soundscapes which lavishly complement their singer\u2019s ecstatic hallucinations. The addition of Brandon Beaver (of Buried Beds, the Silver Ages) allows the group as a 5-piece to revisit its earlier intricate, layered fretwork, while adding new depths of vocal harmonies and ever-peculiar arrangements. Musically, the group hearkens boldly to the raw intensity of 2004's Catch for Us the Foxes, while building on the rich imagery of 2006's Brother, Sister. Epic in scope, Pale Horses is mewithoutYou at their best, breathing fresh life into the end times, gloriously terrifying and hauntingly iconic.\nTheir latest album also marks a new beginning for the band, as it's their first to be released on Run for Cover Records. Teaming with the rising Boston independent label was the outgrowth of their partnership with Will Yip, whose masterful production transforms the band\u2019s transcendental musings into a widescreen experience. Drums and bass lines quake with the faults of the earth, as an army of guitars and multi-instrumental nuances ring in the paranoia, mass hysteria and peaceful exaltation. The result is a stunning collage\u2014fitfully disturbing, steadily bizarre, uniquely celebratory\u2014undoubtedly the grandest musical adventure yet conceived within mewithoutYou\u2019s expanding tapestry.", "otherInfo": "No other event info available"}, {"Address": "217 E Houston St, New York, NY 10002", "Artist": "Field Guides", "ArtistImage": "https://photos.bandsintown.com/thumb/8409827.jpeg", "Coordinates": [-73.986751, 40.722165], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Feely", "Time": "5:30 PM", "Venue": "Mercury Lounge", "moreBioInfo": "Together With Those Which Are of Special Value to the Beekeeper as Sources of Pollen. Songs 'bout people & places & plants & animals.", "otherInfo": "Onlyness, Field Guides, Rose Blanshei"}, {"Address": "500 Grand St, Hoboken, NJ 07030", "Artist": "James Calleo", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-74.035116, 40.743287], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Grand Vin", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "45 Main St, Stanhope, NJ 07874", "Artist": "Crash Gordon", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-74.7085, 40.90277], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "The Stanhope House", "moreBioInfo": "No artist bio available", "otherInfo": "Wayne Hancock tosses out a roots mix of old country, roadhouse blues, western dance swing, boogie bop, and straight up rockabilly!"}, {"Address": "236 E 3rd St, New York, NY 10009", "Artist": "Super King Armor", "ArtistImage": "https://photos.bandsintown.com/thumb/7666035.jpeg", "Coordinates": [-73.981742, 40.722054], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Hip-Hop Rap", "Time": "6:00 PM", "Venue": "Nuyorican Poets Cafe", "moreBioInfo": "Super King Armor, the name of Queens, New York native, & Bi-coastal MC after relocating to Los Angeles for a brief period. Originates from unlikely sources, but they are close to home for him. \"King\" is his Grandmothers last name. \"Armor\" is his Grandfathers last name. Both on his mothers side of the family, whom he considers cornerstones, who have endured a lot and continue to keep moving forward. His name was originally conceptualized out of the desire to have a name that commanded attention & respect. It later began to have more meaning and purpose as he himself would endure both professional and personal challenges that would require a thick skin or suit of armor.\nThe super part of his name was actually tagged on by an organizer of a battle that took place at NYU in the early 2000's. The organizer felt it sounded more threatening, and put it on the flyer. Since then it just stuck. It also has become synonymous with the energy of his live performances. He says. \" I've met people who sincerely hate my name because its either too long, not hood enough, traditional enough, or sounds like it came from a video game , or a cartoon, and laugh because they think its over the top. Those encounters give me a chance to show them something different, how I do my thing, my approach to Hip Hop music. Then there are those people who absolutely love it. They think its the hardest, original name they've heard. Because of that polarity I won't change it. People show there true feelings, have unique responses to it, which starts some interesting conversations and allows me to engage my audience on another level.\" His Approach to Hip Hop can be defined as aggressive, dark, passionate, energetic, gritty, intellectual, witty, creative, uncompromising and autobiographic.\nIf he were to compare himself to a group of artists that inspire, influence, and shares similarities with. They would be Tech N9ne, Andre 3000, Pharoah Monch, Chino XL, Kool Kieth, Rza, Gza, Masta Ace, El-P, Rakim, Eminem, Canibus, Melle Mel, Joe Budden, Nas, Immortal Technique, ATCQ, and Jean Grae. These artists are a mix of Brutally honest, survivors, weird, loner types, perfectionists, passionate lyricists, with a gritty dark sided humor, intellectual, creative, versatile, some independent diy enthusiasts, but most of all driven, hopeless, Hip Hop, romantics.\nSuper King also possesses other distinct talents. One of them being a artist, specifically cartooning, and illustrating. He has interned for Marvel comics and a animation studio in the dumbo area in NYC. With the exception of the album artwork for his EP release \"The Werewolf Lullabies\", and the final stages of the logo for 8th Rank Records, he designed his personal logo, flyers, album covers, storyboards for music videos, & a indie film, and the conceptual art for the 8th Rank Records logo.\nHis other talent is for working out. Having been a personal trainer for a number of years nationally certified, a power lifter, now focusing on calisthenics training, and competitions based around that. He is always willing to offer his skill set through consultation and even training sessions. Just contact him at enraemercerfitness@gmail.com.\nHe is the founder and flagship artist of 8th Rank Records.\nin 2012 he turned it into an LLC. He always wanted to start his own business, and creating his own record label, a place he would have complete autonomy over his music was the obvious route to take. The name was inspired by his love of chess. The 8th Rank is any of the last squares on either side of the board. When one of the pawns, those little pieces in front of all the different larger pieces in the back reaches there, it becomes the strongest piece on the board, being able to move anywhere. A metaphor for going through something, and coming out on the other end in complete control of the direction of where you want to go from there. The snake is a symbol in alchemy referred to as the \"ouroboros\", usually a serpent as a circle eating itself. He wanted multiple meanings to be wrapped in the logo.\nRegarding projects he has released, his horror-core EP \"The Werewolf Lullabies\" Featured production from Eupham from Mindspray Crew, 100DBS, Tah Phrum Duh Bush, and cuts by DJ Milk Money. Artwork provided by Demostina. The Theme of the EP, revolves around a dream he had where in which he transform into a werewolf. The transformation is induced by high volumes of stress, betrayal of friends, and personal demons. The lead singles off the Ep are \" Warganism\", and \"The Most Precious Jewel\". The EP is available on Itunes, Amazon, Google play, and most digital music retailers. After the \"Werewolf Lullabies\" he released several singles. \"Mike Tyson Music\", Eye of The Pupil\", and \"Yada, Yada, Yada. Which you can stream on his reverbnation page. He is currently working on a full length album, release date TBD.\nJune of last year he performed at Sullivan Hall NYC, with his band named after his EP release \"The Werewolf Lullabies\". He created, and co headlined the show titled \"Wrath of the Written\". With rising MC, Nitty Scott MC. He has also toured the Connecticut area, and New Jersey. Upon completion of his album he will be booking extensive tours. Independently,and through festivals, both domestic, and international. Specifically, Canada and Japan.", "otherInfo": "My cypher Knock-Ternal Rhyme Sessions is having a 5hr Showcase of talent at The Nuyorican Poets Cafe'. There will be live beat making and performances by other artists. This is going to be huge!!"}, {"Address": "644 Sackett St, Brooklyn, NY 11217", "Artist": "pellagra", "ArtistImage": "https://photos.bandsintown.com/thumb/9576296.jpeg", "Coordinates": [-73.983533, 40.678302], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Doom Metal, Surf Rock, Punk Metal, Grunge", "Time": "6:00 PM", "Venue": "Lucky 13 Saloon", "moreBioInfo": "Two people got together to make music. It was loud. They needed a third person, so they convinced her to join. It then was louder.\n\nDo you like songs about giant monsters (kaiju), pirates and pirates fighting kaiju? Do you like growling guitars, melodic thumping basslines and drummers that look like Muppets while they play? Well, we're you're goddamn band.", "otherInfo": "Pellagra and Lokane both make their Lucky 13 debut while I, Destroyer plays their first show, anywhere, ever! Come out and see this heavy lineup! $10 in advance, so hit the bands up for tickets! $15 at the door."}, {"Address": "64 N 9th St, Brooklyn, NY 11249", "Artist": "Selfish Things", "ArtistImage": "https://photos.bandsintown.com/thumb/9063557.jpeg", "Coordinates": [-73.95987, 40.720886], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Rough Trade NYC", "moreBioInfo": "We all rot.", "otherInfo": "No other event info available"}, {"Address": "765 Washington St, New York, NY 10014", "Artist": "Yaala Ballin", "ArtistImage": "https://photos.bandsintown.com/thumb/9464409.jpeg", "Coordinates": [-74.008079, 40.737397], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz, Vocal Jazz, great American songbok, singer, Swing", "Time": "6:00 PM", "Venue": "Entwine", "moreBioInfo": "Born in Israel, Yaala Ballin graduated from the prestigious Thelma Yellin High School of the Arts, moving to New York City in 2004 to study with Sheila Jordan.\n\nYaala has recorded 3 CDs \u2013 \u201cTravelin\u2019 Alone\u201d (Smalls Records), \u201cOn The Road\u201d (Gallery Records), \u201cLive Session\u201d (Gut String Records) \u2013 and was recently featured on \u201cThe Music Of Duke Jordan\u201d (Steeplechase Records) with the Chris Byars Septet, for which she also wrote original lyrics.\n\nIn addition to appearences in New York City (Smalls, Smoke, Birdland), she tours frequently in Europe and has been featured at festivals and concert series in the Middle East.\n\nYaala was Vocal Jazz Instructor for the U.S. State Department program \u201cJazz Futures\u201d (2008 \u2013 2013), a six year cultural diplomacy program administered by the U.S. Embassy in Cyprus, and has conducted workshops for the State Department in Ukraine and Turkey.\n\nShe has been on faculty at the City College of New York\u2019s Vocal Jazz Department and at the Center For Jazz Studies at Tel Aviv Conservatory (affiliated with the New School For Jazz and Contemporary Music in New York City), and regularly conducts master classes in the U.S. and internationally.", "otherInfo": "No other event info available"}, {"Address": "570 Jernee Mill Rd, Sayreville, NJ 08872", "Artist": "Drug Church", "ArtistImage": "https://photos.bandsintown.com/thumb/9056136.jpeg", "Coordinates": [-74.354439, 40.440976], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Alternative", "Time": "6:00 PM", "Venue": "Starland Ballroom", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "485 7th Ave, New York, NY 10018", "Artist": "whereisbenny", "ArtistImage": "https://photos.bandsintown.com/thumb/10036630.jpeg", "Coordinates": [-73.985656, 40.661785], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: House, Hip Hop, Reggaeton, Latin Rap", "Time": "6:00 PM", "Venue": "Magic Hour Rooftop Bar & Lounge", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "222 Summer St, Stamford, CT 06901", "Artist": "Acoustic Mafia", "ArtistImage": "https://photos.bandsintown.com/thumb/8644732.jpeg", "Coordinates": [-73.540816, 41.054948], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Acoustic, Rock, Pop", "Time": "6:00 PM", "Venue": "Barcelona Wine Bar", "moreBioInfo": "CT duo. Listen on Spotify, Apple Music & SiriusXM!", "otherInfo": "No other event info available"}, {"Address": "24 Main St., Madison, NJ 07940", "Artist": "Christos Rafalides", "ArtistImage": "https://photos.bandsintown.com/thumb/8692320.jpeg", "Coordinates": [-74.41673, 40.75993], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Shanghai Jazz", "moreBioInfo": "Vibraphonist, Composer, Educator, Producer\n\nwww.manhattanvibes.com", "otherInfo": "Sergio Salvatore, piano\nChristos Rafalides, vibraphone\n\nSpecial guest on vocals\nLuciano Salvatore"}, {"Address": "637 Wyckoff Avenue #115, Madison, NJ 07940", "Artist": "Sergio Salvatore", "ArtistImage": "https://photos.bandsintown.com/thumb/6338616.jpeg", "Coordinates": [-74.43, 40.76], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz, Contemporary Jazz", "Time": "6:30 PM", "Venue": "Shanghai Jazz", "moreBioInfo": "Since he began recording at the remarkable age of 11, Sergio Salvatore's superb musicianship has led to recordings that have featured such notable players as Chick Corea, Michael & Randy Brecker, Gary Burton, Jay Anderson, Danny Gottlieb and William Kennedy, and shows with - among others - Gary Burton, Dave Grusin, Delfeayo Marsalis, Mel Torme, Tony Bennett, and Grover Washington, Jr.\n\nSergio began playing at a very early age and was performing in public by the time he was four. His father, Luciano Salvatore, is a music teacher who majored in classical piano at Boston University and the Berklee College of Music. After teaching at Berklee for five years, Luciano created his own music school North Jersey Music Labs, which specializes in teaching improvisation. It wasn't long before Sergio became his father's star pupil.\n\nEarly in 1992, a home video of one of Sergio's performances came to the attention of the Director of A&R at GRP Records. A live audition at the Salvatore household followed soon after resulting in the recording of Sergio's self-titled debut album for the label, cutting seven original tunes and receiving critically-favorable response to the record.\n\nThe 1994 follow-up on GRP, Tune Up, included compositions by Miles Davis, Charlie Parker, Wayne Shorter, and Chick Corea and led to appearances with the American Jazz Philharmonic and to performances in Japan, Italy, Canada and at selected venues in the U.S. including Carnegie Hall and the Ravinia Jazz Festival. In 1996, Sergio recorded Always A Beginning, an all-acoustic album for Concord Jazz Records that featured his keyboard artistry in a trio setting, alongside bassist John Patitucci and drummer Peter Erskine. Subsequently, Always A Beginning was selected as one of JazzTimes magazine's favorite albums of the year.\n\nFollowing his relationship with Concord Records, Sergio joined the roster of N2K Encoded Music, a New York based media/technology company focusing on music and the Internet. There he recorded his first decidedly contemporary jazz recording produced by Chuck Loeb and including veterans Steve Gadd (drums), Will Lee (bass), John Patitucci (bass), Michael Brecker (tenor saxophone), Bob Mintzer (soprano saxophone), and David Charles (percussion).\n\nIn 2008, Salvatore released Dark Sand, a duet project with virtuoso vibraphonist Christos Rafalides to critical acclaim from major jazz press including Downbeat, JazzTimes, and AllAboutJazz.\n\nIn addition to performing frequently with Mr. Rafalides throughout the world, Sergio has been writing music for a variety of settings including solo piano, trio, and quintet. Mr. Salvatore also holds a Bachelor of Science degree in computer science from Worcester Polytechnic Institute in Worcester, Massachusetts.\n\nSergio Salvatore is a Steinway Artist.", "otherInfo": "Sergio with vibraphonist Christos Rafalides and special guest vocalist, Luciano Salvatore."}, {"Address": "146 Broadway, Brooklyn, NY 11211", "Artist": "Zenizen", "ArtistImage": "https://photos.bandsintown.com/thumb/8178402.jpeg", "Coordinates": [-73.963479, 40.7101], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:30 PM", "Venue": "Baby's All Right", "moreBioInfo": "Created by Opal Hoyt who was born in Alaska and raised between Vermont, Jamaica, Washington DC and Brooklyn. She's infatuated with writing songs and creating musical landscapes with an emphasis on all things soulful. Since heading to Australia for the second time to write/record the band's debut EP, she has been back in New York simultaneously playing shows and working on the band's \"vaguely seasonal\" full-length album--due in 2018. Minnie Riperton, Mariah Carey, and London Nu-jazz is where she came from and ended up. She is committed to modern psychedelia, poetry, and groove. A soul trip", "otherInfo": "~++~"}, {"Address": "146 Broadway, Brooklyn, NY 11211", "Artist": "Carlos Truly", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.963479, 40.7101], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:30 PM", "Venue": "Baby's All Right", "moreBioInfo": "No artist bio available", "otherInfo": "~++~"}, {"Address": "637 Wyckoff Avenue #115, Madison, NJ 07940", "Artist": "Sergio Salvatore", "ArtistImage": "https://photos.bandsintown.com/thumb/6338616.jpeg", "Coordinates": [-74.43, 40.76], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz, Contemporary Jazz", "Time": "6:30 PM", "Venue": "Shanghai Jazz", "moreBioInfo": "Since he began recording at the remarkable age of 11, Sergio Salvatore's superb musicianship has led to recordings that have featured such notable players as Chick Corea, Michael & Randy Brecker, Gary Burton, Jay Anderson, Danny Gottlieb and William Kennedy, and shows with - among others - Gary Burton, Dave Grusin, Delfeayo Marsalis, Mel Torme, Tony Bennett, and Grover Washington, Jr.\n\nSergio began playing at a very early age and was performing in public by the time he was four. His father, Luciano Salvatore, is a music teacher who majored in classical piano at Boston University and the Berklee College of Music. After teaching at Berklee for five years, Luciano created his own music school North Jersey Music Labs, which specializes in teaching improvisation. It wasn't long before Sergio became his father's star pupil.\n\nEarly in 1992, a home video of one of Sergio's performances came to the attention of the Director of A&R at GRP Records. A live audition at the Salvatore household followed soon after resulting in the recording of Sergio's self-titled debut album for the label, cutting seven original tunes and receiving critically-favorable response to the record.\n\nThe 1994 follow-up on GRP, Tune Up, included compositions by Miles Davis, Charlie Parker, Wayne Shorter, and Chick Corea and led to appearances with the American Jazz Philharmonic and to performances in Japan, Italy, Canada and at selected venues in the U.S. including Carnegie Hall and the Ravinia Jazz Festival. In 1996, Sergio recorded Always A Beginning, an all-acoustic album for Concord Jazz Records that featured his keyboard artistry in a trio setting, alongside bassist John Patitucci and drummer Peter Erskine. Subsequently, Always A Beginning was selected as one of JazzTimes magazine's favorite albums of the year.\n\nFollowing his relationship with Concord Records, Sergio joined the roster of N2K Encoded Music, a New York based media/technology company focusing on music and the Internet. There he recorded his first decidedly contemporary jazz recording produced by Chuck Loeb and including veterans Steve Gadd (drums), Will Lee (bass), John Patitucci (bass), Michael Brecker (tenor saxophone), Bob Mintzer (soprano saxophone), and David Charles (percussion).\n\nIn 2008, Salvatore released Dark Sand, a duet project with virtuoso vibraphonist Christos Rafalides to critical acclaim from major jazz press including Downbeat, JazzTimes, and AllAboutJazz.\n\nIn addition to performing frequently with Mr. Rafalides throughout the world, Sergio has been writing music for a variety of settings including solo piano, trio, and quintet. Mr. Salvatore also holds a Bachelor of Science degree in computer science from Worcester Polytechnic Institute in Worcester, Massachusetts.\n\nSergio Salvatore is a Steinway Artist.", "otherInfo": "Sergio with vibraphonist Christos Rafalides and special guest vocalist, Luciano Salvatore."}, {"Address": "45 S 3rd St, Brooklyn, NY 11249, USA, New York City, NY", "Artist": "No Artist", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.965819, 40.713672], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:30 PM", "Venue": "Freehold", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "327 Bowery, New York, NY 10012", "Artist": "Theo Kandel", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.991787, 40.725751], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:30 PM", "Venue": "The Bowery Electric", "moreBioInfo": "No artist bio available", "otherInfo": "Up All Night x GigFinesse Present: Theo Kandel at The Bowery Electric"}, {"Address": "146 Broadway, Brooklyn, NY 11211", "Artist": "Jackie Mendoza", "ArtistImage": "https://photos.bandsintown.com/thumb/9366769.jpeg", "Coordinates": [-73.963479, 40.7101], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "6:30 PM", "Venue": "Baby's All Right", "moreBioInfo": "No artist bio available", "otherInfo": "~++~"}, {"Address": "327 Bowery, New York, NY 10012", "Artist": "Penelope", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.991787, 40.725751], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: 90s, Pop Rock, Decades, Pop", "Time": "6:30 PM", "Venue": "The Bowery Electric", "moreBioInfo": "No artist bio available", "otherInfo": "Up All Night x GigFinesse Present: Theo Kandel at The Bowery Electric"}, {"Address": "1033 6th Avenue, 3rd Floor, New York City, NY 10018", "Artist": "Wait For It", "ArtistImage": "https://photos.bandsintown.com/thumb/7673251.jpeg", "Coordinates": [-73.985902, 40.7527], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Pop, Rock, Pop Punk", "Time": "6:30 PM", "Venue": "Bryant Park Lounge", "moreBioInfo": "Pop/ punk/ rock/ whatever you wanna call it. We call it legend- WAIT FOR IT- dary. True story. Morgantown, WV", "otherInfo": "No other event info available"}, {"Address": "1120 Manhattan Ave, New York, NY 11222", "Artist": "Ghostbound", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.955172, 40.736777], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Saint Vitus Bar", "moreBioInfo": "No artist bio available", "otherInfo": "Join Drownyard as they bring their brand of stoner doom / sludge to Vitus"}, {"Address": "570 Jernee Mill Rd, Sayreville, NJ 08872", "Artist": "Thrice", "ArtistImage": "https://photos.bandsintown.com/thumb/8371908.jpeg", "Coordinates": [-74.354439, 40.440976], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Punk, Post Hardcore", "Time": "7:00 PM", "Venue": "Starland Ballroom", "moreBioInfo": "New album \u2018Palms\u2019 out now! thrice.net", "otherInfo": "No other event info available"}, {"Address": "140 Stewart Ave, Brooklyn, NY 11237", "Artist": "Lights", "ArtistImage": "https://photos.bandsintown.com/thumb/8160890.jpeg", "Coordinates": [-73.926398, 40.710569], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Pop, Electrosoul", "Time": "7:00 PM", "Venue": "Avant Gardner", "moreBioInfo": "Always pushing boundaries, alt-pop phenomenon Lights has never shied away from a challenge. Through her first three records, Lights built an incredibly passionate fanbase, selling out tours around the world, earning 100M in U.S. streams, 200K in U.S. album sales, and two JUNO Awards, not to mention the 2M + rabid fans who follow her every move online. Yet through this success, she felt like she had more important things to say through her music but just didn\u2019t know how to get them out. Thus was born Skin&Earth, the Canadian singer\u2019s fourth record and companion comic book series of the same name. As Billboard explains, \u201cConcept albums are nothing new, but it\u2019s the lengths to which Lights takes her fourth album Skin&Earth that sets it apart,\u201d with Lights having written and illustrated the comic series in full herself while concurrently working on an album to tell the same story. Further critical acclaim for the project comes from Playboy, who dubs her a \u201cReal-life musical wonder woman,\u201d while ELLE describes Lights as \u201ca musician, and artist, and a comic book hero,\u201d Departures praises her as \u201cMore pop siren than indie darling,\u201d and Forbes states Lights is \u201cA creative, enthusiastic force.\u201d Anthemic single \u201cGiants\u201d brings the immersive comic book world to life, as Lights \u2013 and her leading lady \u2013 struggle to find hope in a hopeless world. The single has been steadily climbing the Hot AC radio chart \u2013 currently at #22 \u2013 and has amassed over 2.3M streams on Spotify. As Buzzfeed attests, \u201cIt\u2019s time for Americans to fall in love with Lights.\u201d Skin&Earth is out now: https://lights.lnk.to/skinandearth-album", "otherInfo": "No other event info available"}, {"Address": "327 Bowery, New York, NY 10012", "Artist": "Moon Sugar", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.991787, 40.725751], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "The Bowery Electric", "moreBioInfo": "No artist bio available", "otherInfo": "Moon Sugar in The Map Room"}, {"Address": "185 Orchard St, New York, NY 10002", "Artist": "Eleanor Dubinsky", "ArtistImage": "https://photos.bandsintown.com/thumb/10029797.jpeg", "Coordinates": [-73.988346, 40.722035], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz, Singer, Songwriter, world, pop, Jazz Fusion", "Time": "7:00 PM", "Venue": "Rockwood Music Hall Stage 3", "moreBioInfo": "Eleanor Dubinsky has found the tender edge where jazz, world, and pop sensibilities meet emotionally forthright songwriting. Writing songs in English, French and Spanish, the indie songstress grounds her sound in her soothing, sultry vocals, enlivened by Latin, Brazilian, American and West African rhythms. French Culture Guides named her \u201cBest Up and Coming Francophone Singer in NYC\u201d and her songs have been featured on MTV, Lifetime and Animal Planet, ads for American Express, radio stations throughout Mexico, and on NPR. Eleanor's song \u201cWait For You\u201d won the Sennheiser recording contest at the 2012 ASCAP EXPO. Eleanor has made many festival appearances including the 4 + 4 Days of Movement festival in Prague, The International Festival at Philadelphia\u2019s Kimmel Center for The Performing Arts, the Puerto Morelos Music Festival (Mexico) and concerts in Costa Rica, Portugal, France, Mexico, Cape Verde and Senegal. She performs regularly in New York City, teaches workshops in songwriting and is a teaching artist with Feel The Music! at NY Presbyterian Hospital\u2019s Child And Adolescent Oncology Center.\n\nMost recently, Eleanor suggests we all work to \"Turn It Around,\" the first single from her upcoming third album Soft Spot of My Heart (launching spring 2018). \"Turn It Around,\" co-written with pianist and composer Frank Ponzio, is a soulful tribute to the spring that comes after a long winter, literally and spiritually. To underline her uplifting message, she welcomes the powerful joy of Brooklyn based contemporary gospel ensemble Greg Stamper Stamper & Soul21, who make a noteworthy appearance on two songs on her new record. \"A cut that can only warm your spirits.\" - Digital High\n\nSoft Spot Of My Heart was recorded in Lisbon, Portugal and New York City. For her band, Eleanor selected a group of notable musicians from Cape Verde, Portugal, the USA and Brazil. Each band member brought his/her professional experience and unique cultural references to each song, making Soft Spot Of My Heart a globally savvy collection of tracks that amplify Eleanor's emotional honesty, encourage social action and showcase rhythmic and melodic dexterity.\n\nHear Turn It Around on Spotify: https://open.spotify.com/album/2p7KslpBajhs0dzU1ImEwF\n\nBiografia en espanol: www.eleanordubinsky.com/?page_id=2", "otherInfo": "A joyous, uplifting and deeply generous performance...just the tonic for the frayed nerves of many New Yorkers after another long winter in these parlous times. Go. See her. This is music as medicine for your soul.\"\n- Tom Pryor (Songlines, Afropop Worldwide)\nNew York City based songstress Eleanor Dubinsky has found the tender edge where jazz, world, and pop sensibilities meet emotionally forthright songwriting. Composing in English, French and Spanish, the indie songstress is noted for her soothing, sultry vocals, enlivened by Latin, Brazilian, American and West African rhythms.\nSoft Spot Of My Heart, Dubinsky\u2019s third album, is a globally savvy collection of tracks that foreground her emotional honesty and voice while fully embracing the musical colors and influences of a diverse group of collaborators from Portugal, NYC, Cape Verde and Brazil. This special 15th anniversary concert marks the premiere of Eleanor's 4 Elements Acoustic Quartet. She is joined by bandmates Michael O'Brien (bass), Mayra Casales (percussion/vocals) and Frank Ponzio (keys) in an intimate presentation of her original songs."}, {"Address": "2751 Broadway, New York, NY 10025", "Artist": "Drummer Mike Clark", "ArtistImage": "https://photos.bandsintown.com/thumb/7721160.jpeg", "Coordinates": [-73.96831, 40.80131], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Jazz, Funk", "Time": "7:00 PM", "Venue": "Smoke Jazz & Supper Club", "moreBioInfo": "\u201cHe\u2019s a great jazz drummer\u2014and he hasn\u2019t lost any of the stuff that he brought from Oakland. So now he\u2019s totally free to do both, and he does. The solo he played on the album Thrust, on the song \u201cActual Proof,\u201d is one of the best drum solos on any of my albums. So many people have remarked about that solo, saying, \u2018Incredible.\u2019\u201d\n\n\n\n-Herbie Hancock\n\nMIKE CLARK DRUMMER / BAND LEADER\n\nMIKE CLARK gained worldwide recognition as one of America\u2019s foremost jazz and funk drummers while playing with Herbie Hancock\u2019s group in the early 1970\u2019s. Mike became known as a major innovator through his incisive playing on Hancock\u2019s Thrust album, which garnered him an international cult following.\n\nWhile often referred to as the \u201cTony Williams of funk\u201d, Mike considers himself a jazz drummer, and in fact has become one of the most vital to sit behind a set. He's performed with jazz greats such as Herbie Hancock, Chet Baker, Vince Guaraldi, Tony Bennett, Wayne Shorter, Joe Henderson, Eddie Henderson, Rob Dixon, Bobby Hutcherson, Woody Shaw, Donald Harrison, Larry Coryell, Jack Wilkins, Mike Wolff, Wallace Roney, Geri Allen, Billy Childs, James Genus, Bob Hurst, Chris Potter, Onaje Allan Gumbs, Bobby McFerrin, Al Jarreau, Dave Liebman, Nat Adderly, Oscar Brown Jr., Bill Doggett, Nicholas Payton, Christian McBride, Mose Allison, Maxine Brown, Gil Evans and his orchestra and many illuminating etceteras too numerous to list.\n\nBorn in Sacramento, CA, while growing up Mike had the benefit of traveling around the country with his father, a union man for the railroad. He was also a drummer at one time and had a keen ear, a killing record collection, and a great appreciation for music. This allowed the younger Clark to soak up the diverse regional music of the United States, which he credits as providing an invaluable education. Clark was a prodigy as well, picking up his first sticks at the tender age of five and sitting in--and getting \u201chouse\u201d --with bands in Texas and New Orleans when he was as young as 7 and 8.\n\nHe continued to play in a variety of blues, R&B, organ trios, and jazz bands, settling in Oakland, CA, until his introduction to Herbie Hancock and his recruitment into the Headhunters in 1973.\n\nAt the tail end of the 70\u2018s and in 1980, shortly before moving to New York City, Mike did a short but remarkable stint with Brand X, the acclaimed British fusion band founded by Phil Collins, doing two tours and recording their seminal Do They Hurt? and Product.\n\nThe winter of 2000 saw him diversifying his efforts with a foray into the popular jam band scene. His group Prescription Renewal electrified audiences across the country with an eclectic core of cross- generational multi talents, such as Charlie Hunter, Fred Wesley, Skerik, Robert Walter, and DJ Logic, and featured special guests such as George Porter Jr. of Funky Meters\u2019 fame, Kyle Hollingsworth of String Cheese Incident, Les Claypool, Will Bernard, Larry Goldings and fellow Headhunters alumni, Bill Summers, and proved a hit on the thriving young jam band scene. He followed that with several successful tours of the Roots Funk All Stars, another hot ad hoc association that married Mike with James Brown trombonist Fred Wesley, former Headhunter cohort and funk bassist legend Paul Jackson, Dr. Lonnie Smith and Robben Ford.\n\nNext to James Brown's drummer Clyde Stubblefield, Mike's beats with The Headhunters (most notably on \"God Make Me Funky\") are some of the most sampled in hip hop. Featured in Downbeat, Musician, International Musician & Recording World, Modern Drummer, Jazz Times, Guitar Player, Jazz Is and numerous jazz history and method books, Mike is a former faculty member of Drummers Collective, and is a popular and busy clinician around the country. His book Funk Drumming: Innovative Grooves & Advanced Concepts was published last year by Hal Leonard and is a best seller.\n\nHis numerous recording credits include albums with Herbie Hancock, the Headhunters, Brand X, Eddie Henderson, Jack Wilkins, Mark Puricelli, Mike Wolff, and Alien Army, a group led by guitar great Jack Wilkins. As a bandleader, his release Give The Drummer Some was very enthusiastically received, earning a rare four and a half stars in Downbeat, as was The Funk Stops Here, a joint effort with Hancock alumni Paul Jackson. In 2001, his solo CD, Actual Proof met with critical acclaim, as did the 2003 acoustic jazz release, Summertime, featuring Chris Potter and Billy Childs, which spent many weeks in the top ten charts.\n\nOther efforts included a compilation of his legendary, innovative funk beat samples, Mike Clark\u2014The Headhunter, a re-release of the widely acclaimed Thrust album, and, in 1998, The Return Of The Headhunters, featuring Herbie Hancock. Next came Herbie Hancock\u2019s Box Set, featuring Headhunters classics \u201cActual Proof\u201d and \u201cButterfly,\u201d from Thrust, a few choice cuts from Manchild, and also some historical tracks from the classic Flood, previously only available in Japan. The Headhunters\u2019 newest effort, Evolution/Revolution, released in 2003, featured the latest Headhunter configuration, with original founding members Mike, Bill Summers and Paul Jackson, joined by Donald Harrison, Victor Atkins (Los Hombres Caliente), and Nicholas Payton.\n\nHis 2009 release Blueprints of Jazz, Vol. I produced by Talking House Productions, which featured Christian McBride, Donald Harrison, Patrice Rushen, Jed Levy and Christian Scott, was received with critical acclaim and named one of the best recordings of the decade by Downbeat.\n\nHis latest effort, Carnival of Soul released June 11th by Owl Records and climbing the charts, was described by the Indianapolis Star as \u201ca jazz party with a kaleidoscopic array of players.\u201d Those players include guest turns by renowned country blues singer Delbert McClinton, fellow drumming icon Lenny White, tenor player Rob Dixon, and three separate organ-based ensembles offering up some delicious soul-infused jazz.\n\nA true original, Mike\u2019s work continues to delight and astound his fans around the world.", "otherInfo": "No other event info available"}, {"Address": "327 Bowery, New York, NY 10012", "Artist": "Ampevene", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.991787, 40.725751], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "The Bowery Electric", "moreBioInfo": "No artist bio available", "otherInfo": "Moon Sugar in The Map Room"}, {"Address": "40 Wooster St 3rd floor, New York, NY 10013", "Artist": "Jackson Howard Music", "ArtistImage": "https://photos.bandsintown.com/thumb/9074846.jpeg", "Coordinates": [-74.00262, 40.722275], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Soul-infused acoustic indie", "Time": "7:00 PM", "Venue": "Sofar NYC", "moreBioInfo": "Relaxed, raw, and steeped in classic acoustic troubadours like James Taylor and Jim Croce, Jackson Howard plays and indie-folk/acoustic pop blend with as much attention to detail in the fingerstyle guitar as in the lyrics. A recent transplant to Philadelphia, Jackson has performed at numerous venues around Philadelphia and has toured extensively, performing in 18 U.S. States, Canada, Iceland, Germany, Belgium and the Netherlands.\n\nAfter completing a PhD in Exercise Physiology and spending 2.5 years conducting research on behavioral treatment for obesity, Jackson made the decision to leave academia and concentrate on music full time. This major shift in life trajectory invited a unique perspective that colors his songs.\n\nIn 2017, Jackson released \u201cJust For the Mystery\u201d,recorded at Dark Horse Studios in Franklin, TN. The album has received radio play on over 100 stations across the U.S., and has been licensed by MTV, E!, Discovery, and Netflix for use in their programming. He recently completed a new EP \"New Day Begun\", set to be released on May 10th, 2019 at The Locks @ Sona.", "otherInfo": "No other event info available"}, {"Address": "64 N 9th St, Brooklyn, NY 11249", "Artist": "fredo disco", "ArtistImage": "https://photos.bandsintown.com/thumb/8585416.jpeg", "Coordinates": [-73.95987, 40.720886], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: indie rock", "Time": "7:00 PM", "Venue": "Rough Trade NYC", "moreBioInfo": "hi i'm fredo.\ni am 18.\nmy real name is fredo fosco but my phone always autocorrects it to disco so here we are.\nthese are some songs that i write and record.\nthere are also a couple covers.\nenjoy my tunes, eat some food, and have a good time.", "otherInfo": "No other event info available"}, {"Address": "64 N 9th St, Brooklyn, NY 11249", "Artist": "Young Culture", "ArtistImage": "https://photos.bandsintown.com/thumb/8933520.jpeg", "Coordinates": [-73.95987, 40.720886], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Pop, Rock", "Time": "7:00 PM", "Venue": "Rough Trade NYC", "moreBioInfo": "young culture", "otherInfo": "No other event info available"}, {"Address": "40 Wooster St 3rd floor, New York, NY 10013", "Artist": "Jackson Howard Music", "ArtistImage": "https://photos.bandsintown.com/thumb/9074846.jpeg", "Coordinates": [-74.00262, 40.722275], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Soul-infused acoustic indie", "Time": "7:00 PM", "Venue": "Sofar NYC", "moreBioInfo": "Relaxed, raw, and steeped in classic acoustic troubadours like James Taylor and Jim Croce, Jackson Howard plays and indie-folk/acoustic pop blend with as much attention to detail in the fingerstyle guitar as in the lyrics. A recent transplant to Philadelphia, Jackson has performed at numerous venues around Philadelphia and has toured extensively, performing in 18 U.S. States, Canada, Iceland, Germany, Belgium and the Netherlands.\n\nAfter completing a PhD in Exercise Physiology and spending 2.5 years conducting research on behavioral treatment for obesity, Jackson made the decision to leave academia and concentrate on music full time. This major shift in life trajectory invited a unique perspective that colors his songs.\n\nIn 2017, Jackson released \u201cJust For the Mystery\u201d,recorded at Dark Horse Studios in Franklin, TN. The album has received radio play on over 100 stations across the U.S., and has been licensed by MTV, E!, Discovery, and Netflix for use in their programming. He recently completed a new EP \"New Day Begun\", set to be released on May 10th, 2019 at The Locks @ Sona.", "otherInfo": "No other event info available"}, {"Address": "45 Main St, Stanhope, NJ 07874", "Artist": "Wayne Hancock", "ArtistImage": "https://photos.bandsintown.com/thumb/7032023.jpeg", "Coordinates": [-74.7085, 40.90277], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Juke Joint Swing, Country, Rock", "Time": "7:00 PM", "Venue": "The Stanhope House", "moreBioInfo": "Wayne has been called \"The King of Juke Joint swing,\" a \"Roots Renaissance Man,\" a \"Country Singer's Country Singer\" and \"Hank Williams meets Gershwin.\" But the phrase most frequently echoed throughout his career is \"Wayne Hancock is the real deal.\" Joe Ely said it, Hank Williams III said it, Bobby Koefer from the Texas Playboys said it, as have countless music fans and writers when referring to Hancock's authentic and original blend of honky tonk, western swing, blues and big band that he calls \"juke joint swing!\" Authenticity and sincerity have been the cornerstones of Hancock's writing and music since the start of his career. His refusal to compromise his vision and sell out his music has earned him a fiercely loyal underground following. Hancock's vision, as he puts it, is \"to bring people together and make them feel good about music. It's a spiritual thing and without spirituality, you've got nothin'. There ain't much on the radio that strikes me as being original or from the heart, most of it's from the pocketbook and it shows.\" Wayne is proud of his rural roots and culture and has thoroughly absorbed the spirit of country music's forefathers such as Ernest Tubb, Hank Williams and Jimmie Rodgers. Never a mere imitator, the cutting-edge style that emerges is every bit his own. He breathes youth and driving energy into traditional country forms and adds a dash of big band horns, boogie woogie piano, scorching rockabilly beats, heavenly Hawaiian steel licks and some wigged-out hillbilly jazz guitar. Wayne has a genuine affinity for stomping rockabilly, Western swing, blues, and old-timey country.", "otherInfo": "Wayne Hancock tosses out a roots mix of old country, roadhouse blues, western dance swing, boogie bop, and straight up rockabilly!"}, {"Address": "64 N 9th St, Brooklyn, NY 11249", "Artist": "Young Culture", "ArtistImage": "https://photos.bandsintown.com/thumb/8933520.jpeg", "Coordinates": [-73.95987, 40.720886], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Pop, Rock", "Time": "7:00 PM", "Venue": "Rough Trade NYC", "moreBioInfo": "young culture", "otherInfo": "No other event info available"}, {"Address": "1120 Manhattan Ave, New York, NY 11222", "Artist": "Ghostbound", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.955172, 40.736777], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Saint Vitus Bar", "moreBioInfo": "No artist bio available", "otherInfo": "Join Drownyard as they bring their brand of stoner doom / sludge to Vitus"}, {"Address": "158 Ludlow St, New York, NY 10002", "Artist": "Itay Goldberg", "ArtistImage": "https://photos.bandsintown.com/thumb/9520770.jpeg", "Coordinates": [-73.987746, 40.72104], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Pianos", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "158 Ludlow St, New York, NY 10002", "Artist": "Dejan", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.987746, 40.72104], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Pianos", "moreBioInfo": "No artist bio available", "otherInfo": "Pianos Presents: Dejan, Itay Goldberg\n7pm 21+ FREE"}, {"Address": "140 Stewart Ave, Brooklyn, NY 11237", "Artist": "Callie Reiff", "ArtistImage": "https://photos.bandsintown.com/thumb/9026566.jpeg", "Coordinates": [-73.926398, 40.710569], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Bass Music, Dubstep, Drum Bass", "Time": "7:00 PM", "Venue": "Avant Gardner", "moreBioInfo": "New York-based electronic producer & DJ Callie Reiff is just getting started with her self-made music career, having previously released music on OWSLA\u2019s NEST HQ imprint, A-Trak\u2019s renowned Fool\u2019s Gold label, Ultra Records, & most recently Universal Music. In only a short time, she has already taken NYC by storm performing across major club venues & building a strong foundation within the club circuits.\n\nHer early experiences in the New York City Ballet\u2019s \u2018The Nutcracker\u2019 at 7 years old proved an instant love & attraction with music & performance. Becoming a self-taught drummer watching and learning from YouTube tutorial videos by the age of 10, Callie three years later enrolled in acclaimed NYC electronic music production & DJ School Dubspot, having her first lesson mixing vinyl under the guidance of JP Solis. All while moving forward in her modeling career with NEXT models, gaining global profile with aligned brandssuch as H&M, American Eagle, Converse & Beats by Dr. Dre, as well as editorial spreads for Vogue, Milk & Teen Vogue.\n\nHaving reached almost one million streams on Spotify, Callie\u2019s thrilling debut release \u201cTin Machine\u201d via NEST HQ in 2016 displayed a trendsetting talent within the electronic soundscape. Just a couple months later, she returned to the limelight with her inaugural EP \u2018Shadow Dancing\u2019 through A-Trak\u2019s Brooklyn-based Fool\u2019s Gold Records, including her original title track & a jaw-breaking collaboration with KANDY. The following year she cemented her ability in crafting bass-backed beats & laser-beaming riffs, with her 2017 self-released single \u201cStimulate\u201d. Chiming in elements of live electronica, drumbeat, & ethereal sound design, Callie is a production powerhouse in the making.\n\nWithin this time, Callie has gained global support from the likes of Radio 1\u2019s Annie Nightingale, Diplo and Rinse FM\u2019s Marcus Nasty, while touring New York\u2019s most prominent music venues Output & Terminal 5. Reiff has also made history by becoming the youngest DJ at 15 to have graced the stage at the renowned Webster Hall, opening for Skrillex, Zedd, Ed Sheeran, Wolfgang Gartner & more.\nNotable achievements include fronting Spotify\u2019s \u2018Women of Electronic Music\u2019s\u2019 Playlist for Women\u2019s History Month, as well as delivering remixes for Sofi Tukker, Kayzo, & Ultra Records, as well as being named one of Teen Vogue\u2019s \u201810 female artists who are changing the music game\u2019, displaying her ability to merge both industries together to succeed in a mainstream climate.\n\n2018 has been a phenomenally successful year, with highlights including opening Tomorrowland festival in Belgium, playing a guest mix on Rinse FM alongside Skream, touring with Ghastly as part of his North American run, and capping off the year being part of the official remix album pack for Alison Wonderland with her track \u2018No\u2019. As Reiff prepares to fully launch her self-made name in the limelight, this left field artist is set to take the path less taken in 2019 fronting the Tommy Hilfiger \u2018Make It Possible\u2019 campaign & recently signing with Seven20 Management, establishing the next phase of her professional career .", "otherInfo": "No other event info available"}, {"Address": "116 E. 16th St, New York City, NY 10003", "Artist": "Michael Harrison", "ArtistImage": "https://photos.bandsintown.com/thumb/9620911.jpeg", "Coordinates": [-73.988348, 40.735421], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Upstairs at The Stand", "moreBioInfo": "No artist bio available", "otherInfo": "Privileged Pain!"}, {"Address": "65 4th Ave,, Brooklyn, NY 11217", "Artist": "Dan Perlman", "ArtistImage": "https://photos.bandsintown.com/thumb/9145929.jpeg", "Coordinates": [-73.979574, 40.682148], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Comedy, Standup Comedy", "Time": "7:00 PM", "Venue": "Cherry Tree", "moreBioInfo": "Dan Perlman is a stand-up comedian and writer from New York City. Dan was recently named one of the 2018 New Faces in stand-up at Montreal Just For Laughs and headlined Caroline's on Broadway. Dan is the co-writer/director/star of the award-winning comedy series, Flatbush Misdemeanors, which became Oscar-qualified after winning the Grand Jury Prize for Best Narrative Short at the 2018 Florida Film Festival. Dan created and wrote an animated series, That's My Bus!, which received a pilot order from FOX. Dan has appeared on TruTV, MTV2, and as a cast member on MTV's Vidiots. As a writer-filmmaker, Dan has had four projects featured in the New York Television Festival. Previously, Dan was a writer for SiriusXM's Bennington. He also wrote for the 2017-19 WGA Awards, for hosts including Roy Wood Jr. and Lewis Black. As a stand-up, Dan's festival credits include Montreal Just For Laughs, New York Comedy Festival, SF Sketchfest, Limestone Comedy Fest, and Finger Lakes Comedy Competition (1st place).", "otherInfo": "No other event info available"}, {"Address": "327 Bowery, New York, NY 10012", "Artist": "Lush Honey", "ArtistImage": "https://photos.bandsintown.com/thumb/9720231.jpeg", "Coordinates": [-73.991787, 40.725751], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Rock, Funk, Soul, Jam Bands, Psychedelic Rock", "Time": "7:00 PM", "Venue": "The Bowery Electric", "moreBioInfo": "Brought together by a combination of cosmic coincidences and a shared love for the musical unknown, Lush Honey is a 4-piece psych-soul ensemble that has hailed from the Pioneer Valley of Western Massachusetts since 2016.\n\nDrawing from different backgrounds and an array of influences, Lush Honey combines massive riffs, deep-space grooves, and soaring soulful hooks into their own brand of gooey sonic gold. Live performances, each their own unique experience, feature relentless momentum and cunning improvisations woven spontaneously through tight original arrangements.\n\nDecember 2018 saw the release of APOLLO, Lush Honey's debut full-length release. Available on streaming platforms far and wide, the collection of songs provides a foundation of funk-infused soul on which the band can build their frenetic and ever-evolving live shows.", "otherInfo": "Moon Sugar in The Map Room"}, {"Address": "200 Varick St, New York, NY 10014", "Artist": "J.I.", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-74.00478, 40.72826], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "SOB's", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "1120 Manhattan Ave, New York, NY 11222", "Artist": "MORON", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.955172, 40.736777], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Saint Vitus Bar", "moreBioInfo": "No artist bio available", "otherInfo": "Join Drownyard as they bring their brand of stoner doom / sludge to Vitus"}, {"Address": "116 E. 16th St, New York City, NY 10003", "Artist": "Sarah Cooper", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.988348, 40.735421], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Upstairs at The Stand", "moreBioInfo": "No artist bio available", "otherInfo": "Privileged Pain!"}, {"Address": "116 E. 16th St, New York City, NY 10003", "Artist": "Corinne Fisher", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.988348, 40.735421], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Upstairs at The Stand", "moreBioInfo": "No artist bio available", "otherInfo": "Privileged Pain!"}, {"Address": "40 Wooster St 3rd floor, New York, NY 10013", "Artist": "chrissiemusic", "ArtistImage": "https://photos.bandsintown.com/thumb/10010191.jpeg", "Coordinates": [-74.00262, 40.722275], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Soul, Pop, R&B, soul, Singer, Songwriter", "Time": "7:00 PM", "Venue": "Sofar Sounds", "moreBioInfo": "Chrissie Limos is a Soul/Pop singer and songwriter born in the Philippines and is now residing in North New Jersey. A Senior at William Paterson University, she's majoring in Popular Music Studies.\n\nHer powerful, yet sweet and soulful vocals engage every crowd she performs for throughout the New Jersey and New York Circuit. Her style and sound has compared her from artists like Alicia Keys to Allen Stone.\n\nChrissie plays piano and guitar and composes her own original songs. In her writing, she aims to connect with her listeners and be a voice for those who are unable to speak their emotions, thoughts and struggles.\n\nChrissie Limos has two singles called \"Tricks\" and \"1000 Bullets\". She has recently released her debut EP, Got The Love EP, on November 24. It is now available on all streaming platforms including Spotify and iTunes.\n\nKeep a look out for upcoming videos and show dates!", "otherInfo": "Email chrissie.mgmt@gmail.com for location information."}, {"Address": "1120 Manhattan Ave, New York, NY 11222", "Artist": "Savage Youth", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.955172, 40.736777], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Saint Vitus Bar", "moreBioInfo": "No artist bio available", "otherInfo": "Join Drownyard as they bring their brand of stoner doom / sludge to Vitus"}, {"Address": "4210 Palisades Cte Dr A - 401, West Nyack, NY 10994", "Artist": "Corey Holcomb", "ArtistImage": "https://photos.bandsintown.com/thumb/9620703.jpeg", "Coordinates": [-73.96, 41.1], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "West Nyack Levity Live Comedy Club", "moreBioInfo": "No artist bio available", "otherInfo": "Two item minimum purchase required per person. ALL SALES ARE FINAL Ticket Info:General Admission first come, first seated For Group Sales, CLICK HERE or call (845) 353-5400Got Questions? CLICK HERE"}, {"Address": "552 Grandview Ave, Queens County, NY 11385", "Artist": "Catch Me If You Can", "ArtistImage": "https://photos.bandsintown.com/thumb/9152897.jpeg", "Coordinates": [-73.906959, 40.709159], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Acoustic Alternative, Folk Punk, Rock, PopPunk", "Time": "7:00 PM", "Venue": "Windjammer Bar", "moreBioInfo": "A solo acoustic act hoping to strike the right chord and gain a following. Born and raised on Long Island, I have been surrounded by a thriving music scene my whole life. Being in many bands, I have been able to develop my music skills slowly but surely. Even if this solo act gets me nowhere I never intend to stop writing or playing music for fans, friends, family, or myself.", "otherInfo": "The Upstart Crows\nCuovo\nCatch me if you can"}, {"Address": "140 Stewart Ave, Brooklyn, NY 11237", "Artist": "Jay Robinson", "ArtistImage": "https://photos.bandsintown.com/thumb/9041466.jpeg", "Coordinates": [-73.926398, 40.710569], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Bass Music, Electronic, Electro House", "Time": "7:30 PM", "Venue": "Avant Gardner", "moreBioInfo": "Jay Robinson is your favourite DJs favourite producer. With support for his music arriving from all corners of the electronic music world Jay\u2019s unique brand of bass heavy house has seen him play shows in seminal venues such as the Warehouse Project in Manchester, Printworks in London and the critically acclaimed Le Nuits Sonores in Lyon, where Jay was invited to play a special back to back with techno legend, Laurent Garnier.\n\nJay has releases on some of the worlds biggest labels such as Ministry of Sound, Confessions and Night Bass - and has recently made his debut on the electronic music powerhouse that is Mau5trap.\n\nWith multiple USA tours under his belt and a massive slew of gigs across Europe, Jay has taken his inimitable sound to dance floors on a truly global scale.", "otherInfo": "deadmau5 / CUBE V3 - 2020 Tour - Brooklyn, NY (Sunday)"}, {"Address": "229 W 28th St, New York, NY 10001", "Artist": "BewhY", "ArtistImage": "https://photos.bandsintown.com/thumb/7845864.jpeg", "Coordinates": [-73.99414, 40.74766], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:30 PM", "Venue": "Mission NYC", "moreBioInfo": "No artist bio available", "otherInfo": "The Movie Star himself, BewhY is coming to your city! Kick off your year with the hottest Korean Hip-Hop artist!"}, {"Address": "64 N 9th St, Brooklyn, NY 11249", "Artist": "Have Mercy", "ArtistImage": "https://photos.bandsintown.com/thumb/9883296.jpeg", "Coordinates": [-73.95987, 40.720886], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Emo, Alternative, Indie, Punk", "Time": "7:30 PM", "Venue": "Rough Trade NYC", "moreBioInfo": "In the case of Baltimore's Have Mercy, what wins out-and what ultimately astounds-is raw, unfiltered passion. The band's debut LP, The Earth Pushed Back, was one of the most honest records of 2013-an album that fans of punk and emo from Brand New to Tigers Jaw to Taking Back Sunday simply couldn't afford to miss out on. Produced by Paul Leavitt (All Time Low, The Dangerous Summer, many more), A Place Of Our Own is a refined, more muscular version of what they did well on The Earth Pushed Back - as if they trimmed the fat on some aspects of their sound while showing major growth across the board. It's an organic, sometimes subtle and sometimes very noticeable type of growth. Everyone simply sounds better from an instrumental perspective, and Swindle's gritty vocals have become only more defining this go-round. Since forming in 2011, Swindle feels the band truly has \"grown up as musicians and people and cannot wait for everyone to hear A Place Of Our Own.\" Hopeless Records", "otherInfo": "Have Mercy"}, {"Address": "15 Vandam St, New York, NY 10013", "Artist": "Chronicle", "ArtistImage": "https://photos.bandsintown.com/thumb/8464875.jpeg", "Coordinates": [-74.00995, 40.72446], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Southern Gospel", "Time": "3:00 PM", "Venue": "Huron Club At Soho Playhouse", "moreBioInfo": "We started out as friends, and now make up the group \"CHRONICLE\". We have the same vision to spread the gospel of Jesus Christ through song, and to see souls saved. Three group members make up \"CHRONICLE\" of which there are Tim Kinchen, Greg Sullivan and Missy Kinchen. We primarely sing Southern and Country Gospel Music with tracks, but have a live saxophone and Rhythm Guitar.\nGroup members live in the state of Louisiana, Greg Sullivan lives in Holden and Tim and Missy Kinchen lives in Live Oak . All are members of local churches. We all have conservative christian values. Believe that salvation can only come through Jesus Christ. You may also visit us at www.chroniclegospelgroup.com or www.myspace.com/chroniclesong.\nGod Bless You !!!!!!", "otherInfo": "Feuding tailors, a cocaine-addicted rabbi and a Jewish woman who joins a fundamentalist Christian sect are just a few of the sometimes poignant, often humorous stories told through music in singer-songwriter Daniel Cainer's Jewish Chronicles. Winner of an Edinburgh Fringe Award, Cainer's one-man musical journey has been a smashing success from London to New York to Sydney thanks to his deft storytelling ability and virtuoso piano chops. You'll laugh, cry and thrill to this one-man masterpiece, which the U.K.'s Daily Mirror called a must see."}, {"Address": "233 West St, New York, NY 10013", "Artist": "Aztec Two-Step", "ArtistImage": "https://photos.bandsintown.com/thumb/9391497.jpeg", "Coordinates": [-74.012344, 40.721325], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "Genres: Folk, Rock, Singer-Songwriter, Pop-Folk", "Time": "7:30 PM", "Venue": "City Vineyard", "moreBioInfo": "It's a new era for the renowned, award-winning Aztec Two-Step.\n\nFollowing partner Neal Shulman\u2019s 2018 retirement, original founding member, principal singer and songwriter Rex Fowler continues on with their captivating catalog (including some of Neal's originals), expanding the band's musical legacy with his new ensemble AZTEC TWO-STEP 2.0.\n\nWith 2 and 3-part harmonies, multi-instrumental arrangements, and Dodie Pettit\u2019s electrifying guitar work, Aztec Two-Step 2.0 recaptures the sound and feeling of Rex & Neal\u2019s original studio recordings. They perform ATS fan favorites and forgotten gems, with the stage patter still as engaging, fun, and poignant as ever!\n\nWhat people are saying:\n\u201cWOW!\u2026seriously FABULOUS!\u201d\n\u201cWhat a wonderful show!! Exceeded all my expectations! Just magical!!\u201d\n\"Absolutely FANTASTIC show last night. All familiar material, just taken up a notch!! Really, really terrific show! Great job Rex and company!!\"\n\"I went to the show\u2026. Wasn't sure what to expect. How do you replace Neal's guitar work? What a pleasant surprise!! Rex was Rex and the band was awesome!....Don't miss the opportunity to see them!\"\n\nRex\u2019s new band members:\n\nDodie Pettit - Acoustic & Electric Guitars, Keyboards, Vocals\nAccomplished songwriter and recording artist, Dodie Pettit first met Rex Fowler in 1979 when she was a session player and singer on ATS\u2019s \"Times of Our Lives\u201d album. A member of the famed The Untouchable and The Enchanted Forest All-Girl Bands beginning in the mid-1960s, Dodie was a former Principal Dancer for the American Repertory Ballet which led to a distinguished 15-year career on Broadway as an original cast member of The Phantom of the Opera, as well as appearing in Tony Award winning musicals CATS and Titanic. Dodie and Rex were recently married and together, are the heart and soul of Aztec Two Step 2.0.\n\nSteven \u201cMuddy\u201d Roues - Upright Bass, Blues Harmonica, Vocals\nRecord producer, songwriter, recording artist and touring musician, Muddy has had a rich and fulfilling 40-year career in music. He continues to perform his original roots music regularly with The Roues Brothers, Finn & The Sharks and The UpSouth Twisters. Muddy has performed live or recorded with luminaries, B.B. King, Bo Diddley, Howling Wolf, James Cotton, Sam and Dave, The Chambers Brothers, Wilson Pickett, John Hiatt, Joe Ely and David Bromberg. Muddy was an honored band member in the critically acclaimed documentary film, \u201cThe Other Side of Nashville\u201d featuring Carl Perkins.\n\nPeter Hohmeister - Drums, Percussion, Vocals\nRex discovered Peter when he heard him drumming with an exquisite jazz quartet led by multi-instrumentalist Joe Meo. Over the years Peter has performed as both a touring musician and as a member of several Connecticut orchestral and theater programs including the Hartford Symphony. He is currently the principal percussionist with the Norwalk Symphony. Peter is a revered and accomplished musician and teacher, with a nuanced and versatile drumming style. His versatility is a perfect fit for ATS's diverse musical canon.\n\nJoe Meo - Tenor and Soprano Sax, Flute, Clarinet, Vocals\nJoe began playing music at the age of 8. His first instrument, a clarinet, fell from a car and landed in his front yard. He played in musical groups in school but his main experience came from plying in a student led community big band, learning saxophone, flute and guitar along the way. He has since played in pit orchestras for Broadway shows, as well as in rock, funk and jazz bands, and as a session player with distinguished artists Johnny Winter, Julie Budd, Johnny Mathis, Billy Vera, the Temptations, the Four Tops and the Pointer Sisters. Joe is the newest member of ATS 2.0, adding virtuoso solos and supporting lines that compliment and support Dodie\u2019s compelling lead guitar work, and he has been known to step out and sing lead on a song or two as well.", "otherInfo": "No other event info available"}, {"Address": "233 West St, New York, NY 10013", "Artist": "Singer-Songwriter", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-74.012344, 40.721325], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "7:30 PM", "Venue": "City Vineyard", "moreBioInfo": "No artist bio available", "otherInfo": "Big arena concerts are great, but there's nothing quite like the magic of an intimate performance from a singer-songwriter -- no smoke machine or lasers, just heartfelt music. That's where City Winery's Voices on the Hudson series comes in. City Vineyard at Pier 26 will host a slew of favorite City Winery artists. Enjoy a night of music set against the scenic backdrop of the Hudson River and downtown New York City skyline. Make a night of it by ordering dinner from the seasonal menu before or during the show."}, {"Address": "25 Avenue A, New York City, NY 10009", "Artist": "Phil and the Osophers", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.985959, 40.722966], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "8:00 PM", "Venue": "Berlin Under A", "moreBioInfo": "No artist bio available", "otherInfo": "The Bad Man in New York City"}, {"Address": "487 Atlantic Avenue, Brooklyn, NY 11217", "Artist": "Mike Cannon", "ArtistImage": "https://photos.bandsintown.com/thumb/9616538.jpeg", "Coordinates": [-73.981649, 40.685861], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "8:00 PM", "Venue": "EastVille Comedy Club - Brooklyn", "moreBioInfo": "No artist bio available", "otherInfo": "Janeane Garofalo, Mike Drucker, Rachel Feinstein, Jordan Carlos, Mike"}, {"Address": "702 Union St, Brooklyn, NY 11215", "Artist": "Phoebe Robinson", "ArtistImage": "https://photos.bandsintown.com/thumb/9619241.jpeg", "Coordinates": [-73.980048, 40.676115], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "8:00 PM", "Venue": "Union Hall", "moreBioInfo": "No artist bio available", "otherInfo": "Phoebe Robinson"}, {"Address": "217 E Houston St, New York, NY 10002", "Artist": "Marron", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.986751, 40.722165], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "8:00 PM", "Venue": "Mercury Lounge", "moreBioInfo": "No artist bio available", "otherInfo": "Fansu, Xol Azul"}, {"Address": "307 W 26th St, New York, NY 10001", "Artist": "Noah Britton", "ArtistImage": "https://photos.bandsintown.com/thumb/172776.jpeg", "Coordinates": [-73.997414, 40.747469], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "8:00 PM", "Venue": "Improv Asylum New York", "moreBioInfo": "As of right now, the last living person to type something onto Bandsintown", "otherInfo": "Noah Britton (HBO, Netflix) tells a hilarious and shocking story no one has heard, \u201cHarrison.\u201d It\u2019s about false identity, Facebook, and IRL performance art. Everyone who can\u2019t keep a secret will be asked to leave after Jamie\u2019s set.\n\nJamie Loftus (co-headliner) is a writer, stand-up comic, and animator who in 2018, Vulture called one of the Best New Comedians to Watch. Most recently a writer on the Adult Swim series, \u201cRobot Chicken,\u201d Jamie currently co-hosts the HowStuffWorks feminist movie podcast, \u201cThe Bechdel Cast.\u201d Jamie is a regular contributor to publications such as Vice, Paste, The New Yorker, and Playboy.\n8 PM: Jamie Loftus\n8:30 PM: Noah Britton\n$15"}, {"Address": "116 E 16th St, New York, NY 10003", "Artist": "Nikki Carr", "ArtistImage": "https://photos.bandsintown.com/thumb/9616342.jpeg", "Coordinates": [-73.988348, 40.735421], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "8:00 PM", "Venue": "The Stand", "moreBioInfo": "No artist bio available", "otherInfo": "No other event info available"}, {"Address": "487 Atlantic Avenue, Brooklyn, NY 11217", "Artist": "Mike Drucker", "ArtistImage": "https://assets.bandsintown.com/images/fallbackImage.png", "Coordinates": [-73.981649, 40.685861], "Date": "Sun Feb 9 2020", "EventDate": "2020-02-09", "Genre": "No genre available", "Time": "8:00 PM", "Venue": "EastVille Comedy Club - Brooklyn", "moreBioInfo": "No artist bio available", "otherInfo": "Janeane Garofalo, Mike Drucker, Rachel Feinstein, Jordan Carlos, Mike"}, {"Address": "487 Atlantic Avenue, Brooklyn, NY 11217", "Artist": "Rachel Feinstein", "Ar